<script type="text/javascript">
var chkForm = {
	msg: {
		title_empty: vaildForm.msg["title_empty"],
		title_long: vaildForm.msg["title_long"],
        qType_empty:vaildForm.msg["type_empty"],
		content_empty: vaildForm.msg["content_empty"],
	    content_long: vaildForm.msg["content_long"],
	    email_empty: vaildForm.msg["email_empty"],
        email_err: vaildForm.msg["email_err"],
	},
	checkType: function(formIndex){
		var formO = window["form" + formIndex]["formO"];
		var types = formO[0]["type"];
		var selType = false,
			errMsg = chkForm.msg.qType_empty;
		for(var i = 0, l = types.length; i < l; i ++){
			if(types[i].checked){
				selType = true;
				errMsg = null;
				break;
			}
		}
		vaildForm.setChkMsg("form"+ formIndex +"_type", errMsg);
		return selType;
	}
};

var form1 = {
	formO : baidu("#tousu_form1"),
	init: function(){
		this.bindCheck();
	},
	bindCheck: function(){
		var formO = this.formO;
		var formMsg = chkForm.msg;
		formO.vaildForm({
			fields:{
				"#form1_title": {
					required:true,
			        regExp:"len",
			        emptyMsg: formMsg["title_empty"],
			        errMsg: formMsg["title_long"]
				},
				"#form1_email":{
			        regExp:"email",
			        errMsg: formMsg["email_err"]
				},
				"#form1_content":{
					required:true,
			        regExp:"len",
			        emptyMsg: formMsg["content_empty"],
			        errMsg: formMsg["content_long"]
				}
			},
			submitCbk: function(rlt){
				var selType = chkForm.checkType("1");
				rlt = rlt && selType;
				return rlt;
			}
		});
	}
};

var form2 = {
	formO : baidu("#tousu_form2"),
