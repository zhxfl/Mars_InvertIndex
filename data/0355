NTES.ready(function() {
    function MicroBlog(wraper) {
        this.wraper = wraper;
        this.listTweets = this.wraper.$(".list-tweet > li");
        this.currentList = this.listTweets.$(0);
        var t = this;
        this.listTweets.each(function(i, v) {
            NTES.event.Mouseenter("mouseenter", function() {
                t.currentList.removeCss("eat-me");
                t.currentList.addCss("drink-me");
                t.currentList = $(this);
                t.currentList.addCss("eat-me");
                t.currentList.removeCss("drink-me");
            }, $(v))
        });
    }
    new MicroBlog($("#layout-t"))
})();
