(function(){
    NTES.ajax.importJs('http://news.163.com/special/hot_tags_recommend_data/', function(){
        if (hotTagsData.count && hotTagsData.count > 0) {
            var code = '#from=www_tag',
                    html = [],
                    item = { name: '', url: ''};
            for (var i = 0; i < hotTagsData.data.length; i++) {
                item = hotTagsData.data[i];
                html.push('<a href="' + item.url + code + '">' + item.name + '</a>');
            }
            NTES('#js_hotTag').innerHTML = html.join(' ');
        }
    }, 'gbk');
})();
(function(){
    var api_interest = "http://api.pr.163.com/rec/www2?callback=jsonp",newsData = [];
    window.jsonp = function(data){
        newsData = data;
    };
    NTES.ajax.importJs(api_interest,function(){
        var re = /\'|\"/gi;
        var datalen = newsData.length,_flag = 0;
    function refreshdata(){
       if(_flag > 14){
              _flag = 0;
           }
           var html = [];
           for (var i = _flag; i < _flag + 7; i++){
               var alttitle = newsData[i].title.replace(re, "&quot;");
               html.push('<li><a href="' + newsData[i].link + '" title="'+ alttitle +'">' + newsData[i].title + '</a></li>');
           }
           _flag = _flag + 7
           NTES('#js_interst_ul').innerHTML = html.join('');
     }
         if(datalen == 21){
        refreshdata();
            var _img =  $("#js_refresh_img");
            $($("#layout-recommend .tab-hd-con a")[0]).addCss("refresh-pre-text");
            _img.style.display = "block";
            _img.addEvent("mouseover",function(e){ 
               this.src="http://img1.cache.netease.com/img12/public/refresh1_.png";
            });
            _img.addEvent("mouseout",function(e){ 
               this.src="http://img1.cache.netease.com/img12/public/refresh_.png";
            });
            _img.addEvent("click",function(e){ 
                e.preventDefault();
                refreshdata();
            });
         }
        
    }, 'utf-8');
})();
