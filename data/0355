    (function(NTES,window,undefined){var util=NTES.util,extend=util.extend,loggerPath="",doc=document,db=doc.body,de=doc.documentElement,count=0,loggerQueue=[],startQuery="",startSuccess=false,startFailCount=0,timelineReg=/(start|ready|load)/i,defaults={project:"",ref:"NTES_FE_LOGGER",type:"",module:"",message:"null",screen:"",scroll:"",time:(new Date).getTime},log=function(data){if(!db){db=doc.body}var name,params=[],reqInfo=[];data=extend(extend(extend({},defaults),data||{}),{time:(new Date).getTime()});if(data.timeAdjust){data.time=data.time+data.timeAdjust;delete data.timeAdjust}if(timelineReg.test(data.type)){data.message=data.time-data.message}if(db){data=extend(data,{screen:[window.innerWidth,"/",window.innerHeight].join(""),scroll:[db.scrollTop||de.scrollTop,"/",db.scrollLeft||de.scrollLeft].join("")})}for(name in data){params.push([name,data[name]].join("="))}reqInfo.push(loggerPath,"?",params.join("&"));if(!startSuccess){if(data.type=="start"){sendLog(reqInfo.join(""),function(){startQuery="";startSuccess=true;while(loggerQueue.length){sendLog(loggerQueue.shift())}})}else{loggerQueue.push(reqInfo.join(""))}return}sendLog(reqInfo.join(""))},sendLog=function(url,cb){var image=null,tmp="";tmp=["js_logger_image_",String(++count)].join("");window[tmp]=image=new Image;image.onload=function(){image.onload=null;window[tmp]=image=null;cb&&cb()};image.onerror=image.onabort=function(){image.onerror=image.onabort=null;window[tmp]=image=null;if(cb&&startFailCount++<5){setTimeout(function(){sendLog(url,cb)},20)}};image.src=url},setConfigure=function(configs){if(configs){configs.loggerPath&&(loggerPath=configs.loggerPath);configs.ref&&(defaults.ref=configs.ref);configs.project&&(defaults.project=configs.project)}},raccon={log:function(data){log(data);return raccon},setConfigure:function(configs){setConfigure(configs);return raccon}},init=function(){window.NTESLogger=window.NTESLogger||raccon};init()})(NTES,window);
</script>
