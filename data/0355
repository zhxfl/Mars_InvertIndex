			 },{  
             name: "福建", 
             url:"http://fj.house.163.com/xf",
             str:"fj" 
			 },{  
             name: "唐山", 
             url:"http://ts.house.163.com/xf",
             str:"ts" 
			 },{  
             name: "合肥", 
             url:"http://hf.house.163.com/xf",
             str:"hf" 
			 },{  
             name: "南宁", 
             url:"http://gx.house.163.com/xf",
             str:"gx" 
			 },{  
             name: "南京", 
             url:"http://nj.house.163.com/xf",
             str:"nj" 
			 },{  
             name: "淄博", 
             url:"http://zibo.house.163.com/xf/",
             str:"zibo" 
			 },{  
             name: "泰州", 
             url:"http://tz.house.163.com/xf/",
             str:"tz" 
			 },{  
             name: "大庆", 
             url:"http://daqing.house.163.com/xf/",
             str:"daqing" 
			 },{  
             name: "牡丹江", 
             url:"http://mudanjiang.house.163.com/xf/",
             str:"mudanjiang" 
			 },{  
             name: "齐齐哈尔", 
             url:"http://qiqiqhaer.house.163.com/xf/",
             str:"qiqiqhaer" 
			 },{  
             name: "佳木斯", 
             url:"http://jiamusi.house.163.com/xf/",
             str:"jiamusi" 
			 },{  
             name: "鸡西", 
             url:"http://jixi.house.163.com/xf/",
             str:"jixi" 
			 },{  
             name: "温州", 
             url:"http://wz.house.163.com/xf/",
             str:"wz" 
			 }
        ],
        province: [{
            name: "北京市",
            str:"bj"
        }, {
            name: "上海市",
            str:"sh"
        }, {
            name: "河北省",
            str:"sjz"
        }, {
            name: "安徽省",
            str:"hf"
        }, {
            name: "福建省",
            str:"fj"
        }, {
            name: "广西",
            str:"gx"
        }, {
            name: "江苏省",
            str:"nj"
        }],
        defaultCity: {//默认为广州
            str:"gz"
        }
	}
	var SetLocalBuyHouse = function(str){
		var len = BuyHouseConfig.city.length;
		var url;
		var i = -1;
		while(++i < len){
			if(BuyHouseConfig.city[i].str == str){
				url = BuyHouseConfig.city[i].url;
				$("#buyHouse").href = url;
			}
		}
	}
NTES.ajax.importJs(HouseConfig.ip, function(){
	var clen = BuyHouseConfig.city.length , plen = BuyHouseConfig.province.length;
	var locStr = "";
    var j = -1;
    while (++j < clen) {
        if (localAddress.city.indexOf(BuyHouseConfig.city[j].name) != -1) {
           locStr = BuyHouseConfig.city[j].str;
        }
    }
    if(!locStr){
       var i = -1;
            while(++i < plen){
                  if(BuyHouseConfig.province[i].name.indexOf(localAddress.province) != -1){
                       locStr = BuyHouseConfig.province[i].str;
                   }
             }
     }
    if (!locStr) {
       locStr = BuyHouseConfig.defaultCity.str;
    }
	SetLocalBuyHouse(locStr);
});
            //end
        }
    }
    House.init();
});
