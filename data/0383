//Wait for DOM Ready
Q.element.ready(function(){
	var cupid = Q.$('*[data-widget-adparam=public]').attr('data-adparam-cupid_ares');
	var playerId = '';
	if (Q.browser.iPad) {
		playerId = 'qc_100001_100141';
	} else {
		playerId = 'qc_100001_100025';
	}
	if (cupid && playerId) {
		seajs.use(cupid, function(ADC){
			var adc = new ADC(playerId);
		});
	}
});
</script>
