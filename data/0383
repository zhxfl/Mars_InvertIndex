<script type="text/javascript"> 
    /*焦点互动播放器 开始*/
  	var adData = [	];
    /*焦点互动播放器 结束*/
    function openFocusAd(data) {
		if(!Q.browser.iPad) {
			if(data && data.type == 'play') {
				for(var i = 0, j = adData.length; i < j; i++) {
					if(adData[i].frame == data.frame) {
                            var flashstr = getFlashStr('1004','411',adData[i].url);
                            document.getElementById('adflash').innerHTML = flashstr;
                            document.getElementById('adflash').style.display = 'block';
				            return true;
					}
				}
			}
		}
		return false;
	}
    /*触发巨幕 开始*/

   /*触发巨幕 结束*/
   
  /*普通巨幕 开始*/    
      
    
    if(!Q.browser.iPad){
        var qycrazyjm = Q.cookie.get('jm_1379984889101');
        qycrazyjm = qycrazyjm == null ? 0 : qycrazyjm;
        if(qycrazyjm < 2) {
            var flashstr = getFlashStr('1004','411','http://www.iqiyi.com/player/20130917153838/1004410.swf');
            document.getElementById('adflash').innerHTML = flashstr;
            document.getElementById('adflash').style.display = 'block';
            var d = new Date();
            var s = d.getFullYear()+ '/' + (parseInt(d.getMonth()) + 1) + '/' + (parseInt(d.getDate()) + 1) + ' 00:00:00';
            s = new Date(Date.parse(s)) - d;
            Q.cookie.set('jm_1379984889101', ++qycrazyjm,{path:"/",expires:s,domain:"www.iqiyi.com"});
        }
    }



	/*普通巨幕 结束*/
    function closeCrazy(id) {var id='adflash';document.getElementById(id).style.display = 'none';document.getElementById(id).innerHTML = '';}
    function getFlashStr(width, height, adurl) {var objectStr = '<object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000"';objectStr += 'width="' + width + '" height="' + height + '" align="middle" id="' + Math.random() + '">';objectStr += '<param name="movie" value="' + adurl + '" />';objectStr += '<param name="quality" value="high" />';objectStr += '<param name="wmode" value="transparent" />';objectStr += '<param name="allowscriptaccess" value="always" />';objectStr += '<embed src="' + adurl + '" quality="high" width="' + width + '" height="' + height + '" align="middle" wmode="transparent" allowscriptaccess="always" type="application/x-shockwave-flash" /></object>';return objectStr}
</script>
