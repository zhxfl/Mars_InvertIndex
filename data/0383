    <!--js-->
	
 
<span style="display:none;" data-responsivelayout-param="medium=_1279&large=1280_1439&larger=1440" data-widget-responsivelayout=responsivelayout>
<script type="text/javascript">
	(function(){
            var body = Q.$('body');
            var responsiveLayout = Q.$('[data-widget-responsivelayout=responsivelayout]');
            if(!responsiveLayout)
                return;
            Q.$('html').css('overflowY', 'scroll');
            var param = responsiveLayout.attr('data-responsivelayout-param');
            param = Q.url.queryToJson(param);
            var clsConfig = [];
            for (var i in param) {
                var w = param[i].split('_');
                var cls = {'cls': i};
                if (w[0] && w[0] != ''){
                    cls.min = parseInt(w[0]);
                }else{
                    cls.min = 0;
                }
                if (w[1] && w[1] != ''){
                    cls.max = parseInt(w[1]);
                }else{
                    cls.max = 9999;
                }
                clsConfig.push(cls);
            }
            for(var i = 0; i < clsConfig.length; i++)
            {
                if(body.hasClass(clsConfig[i].cls));
                body.removeClass(clsConfig[i].cls);
            }
            var width = Q.page.getViewWidth()>Q.page.getWidth()?Q.page.getWidth():Q.page.getViewWidth();
            for(var i = 0; i < clsConfig.length; i++)
            {
                if(clsConfig[i].min&&clsConfig[i].min>width)
                    continue;
                if(clsConfig[i].max&&clsConfig[i].max<width)
                    continue;
                body.addClass(clsConfig[i].cls);
                break;
            }
        })();
</script>
</span>
