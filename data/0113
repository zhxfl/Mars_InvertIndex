    <script type="text/javascript">

		function showVotePopup () {
			var $votePopup = $("#vote_popup"),
				$close = $votePopup.find(".vote-popup-close");

			$votePopup.floatLayer().floatLayer("show");
			$close.click(function () {
				$votePopup.floatLayer("hide");
				return $(this).hasClass("down");
			});
		}

		function vote (uid, callback, $dom) {

			var baseURL = "/xiaoyuan/vote?t=" + (new Date * 1) + "&singer_uid=",
				voteURL = baseURL + uid;

			if ($dom && $dom.length) {
				if ($dom.data("voting"))	{
					return false;
				}
				$dom.data("voting", true);
			}

			$.ajax({
				url: voteURL,
				type: "get",
				dataType: "jsonp",
				success: function (result) {
					switch (parseInt(result.error, 10)) {
						case 22000:
                            if ($dom && $dom.length) {
								$dom.tip().tip("tipup", { msg: "加人气成功", iconClass: "tip-success" });				
							}

							callback && callback(result);		
							break;
						case 22001:
							break;
						case 23000:
							showVotePopup();
							break;
					}

					$dom.data("voting", false);
				}	
			});
		}

		function initGlobalVote () {
            
            $(document).on('click', '.vote-btn:not(".disabled")', function() {
            // console.log('vote!');
            var that = $(this),
                singerId = that.metadata().singerId;
              // console.log(singerId);
                  vote(singerId, function () {
                    var $voteCount = that.next(".vote-count");
                    that.addClass('voted-btn');
                    if ($voteCount) {
                      $voteCount.text(1 + parseInt($voteCount.text(), 10));

                    }
                  }, that);

                  return false;  
            });

            $(document).on('click', '.disabled.vote-btn', function() {
                $(this).tip().tip("tipup", { msg: "投票已结束", iconClass: "tip-warning" });       
                return false;
            });
		}

        function checkUserEntry(){
            $(".hd-entry").click(function(){
                                //未登录
//                $("#loginbtn").trigger('click');
                ting.passport.checkLogin(function(userinfo){
                    window.location.reload();
                },function(){
                    //onFailure
                },function(){
                    //cancel
                },function(){
                    //onpassportsucess
                }, {});
                
            });
        }
        function search() {
            var $form = $('#search_panel'),
                $input = $('#searchHeader');
            $form.on('submit', function() {
                var val = $.trim($input.val());
                return !!val;
            });
        }

        function songShare(){
            var $curShare = null,
                time = null;
            $(".song-share").each(function(){
                var $share = $(this),
                    $btn = $share.find('.btn-share'),
                    $panel = $share.find('.baidu-share-panel');
                $share.on({
                    mouseenter: function(){
//                        console.log('enter');
                        if ($curShare && $curShare[0] === $(this)[0] && time) {
//                            console.log("clearTimeout");
                            clearTimeout(time);
                            time = null;
                        }
                        $btn.addClass('show-active');
                        $panel.show();
                    },
                    mouseleave: function(e){
//                        console.log("leave");
                        $curShare = $(this);
//
                        if ($(e.relatedTarget).parents('#bdshare_s').length > 0) {
                            return;
                        }
                        time = setTimeout(function() {
                            $btn.removeClass('show-active');
                            $panel.hide();
                        }, 500);
                    }
                });
                $panel.find('.bdshare_b').on('mouseenter', function() {
                    $share.trigger('mouseenter');
                });
            });
            $(document).on('mouseleave', '#bdshare_s', function() {
                // 避免panel先消失，导致分享panel闪烁
                setTimeout(function() {
                    $curShare.find('.btn-share').removeClass('show-active');
                    $curShare.parent().find('.baidu-share-panel').hide();
                }, 0);
            });
        }

		function initEntryDialog () {
			$("#entry-dialog").dialog( {
				confirm : {
					btn : ".confirm",
					callback: function() {
						$("#entry-dialog").dialog("close");
					} 
				},
				close: {
					callback: function() {
						$("#entry-dialog").dialog("close");
					}
				}
			} );
		}

        $(function() {
//            userLogin();
			initEntryDialog();
            checkUserEntry();
            $('#searchHeader').watermark();
            search();
			initGlobalVote();
        });
    </script>

