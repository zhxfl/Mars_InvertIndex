	display_by_ip_instance_num++;
	var display_by_ip_sign = display_by_ip_seed+display_by_ip_instance_num;
 
	eval("var display_by_ip_"+display_by_ip_sign+" = new DispImgTxtByIp(display_by_ip_img_array, display_by_ip_txt_array, display_by_ip_scroll, parseInt(display_by_ip_scroll_delay), display_by_ip_sign, sohu_IP_Loc_t)");
</script>
<script> 
	eval("display_by_ip_"+display_by_ip_sign+".ImgShow()");
</script>
