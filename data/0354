                                     <script>
                                (function(){
                               var _$ = function(selecter) {
                                   return document.getElementById(selecter);
                               }
                               _$.isVisable = function(ele) {
                                   if (ele.style.display == "" || ele.style.display == "none") {
                                       return false
                                   }
                                   return true;
                               }
                               _$.hide = function(ele){
                                   ele.style.display = "none";
                               }
                               _$.show = function(ele) {
                                   ele.style.display = "block";
                               }
                   
                   
                               var _sele_now =  _$('sele_now_focus'),
                                   _sele_con = _$('sele_con_focus'),
                                   _sele_ul = _$('sele_ul_focus');
                                   _input_select = _$('select_input_focus');
                   
                               (function bindEventForDOM() {
                                   /*--hide or show sele_content ---begin--*/
                                   _sele_now.onclick = function() {
                                       _$.isVisable(_sele_con) ? _$.hide(_sele_con) : _$.show(_sele_con);
                                   };
                   
                                   if(window.addEventListener){
                                                                 document.addEventListener("click",eventHandler,false);
                                                        }else{
                                                                 document.attachEvent("onclick",eventHandler);
                                                        }
                                                        
                                                        function eventHandler(e) {
                                       var event = e || window.event;
                                       var target = event.target || event.srcElement;
                                       if (target.getAttribute('no-hide') != null) return true;
                                       _$.hide(_sele_con);
                                   }
                   
                                   /*--input:text focus--*/
                                   _input_select.onmouseover = function(){
                                       _input_select.focus();
                                   }
                               })();
                   
                               /**
                                * [bindEventForForm : set input value for form]
                                */
                               (function bindEventForForm() {
                                   _sele_ul.onclick = function(e){
                                       var event = e || window.event;
                                       var target = event.target || event.srcElement;
                                       var url = target.getAttribute('data-url');
                                       if ("A" != target.tagName|| !url) return false;
                                       _sele_now.innerHTML =target.innerHTML;
                                       _$('focusD_query').setAttribute('action', url);
                                       _$.hide(_sele_con);
                                   }
                               })();
                                       })();
