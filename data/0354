				<script type="text/javascript">
					function changeSpsmn(n){
						if (n == 0) {
							$("spsb1").className = "titleA spstab_1";
							$("sportstab1").className = "show";
							$("sportstab2").className = "hidden";
						}else{
							$("spsb1").className = "titleA spstab_2";
							$("sportstab1").className = "hidden";
							$("sportstab2").className = "show";
						}
					}
				</script>
