                                     <script type="text/javascript">
                                     void (function(window, document, undefined){
                                               //add 20130813 by @fzxa
                                               "use strict";
                                               
                                               function _$(id){
                                                        return document.getElementById(id);
                                               }
                                               
                                               var carBm = {
                                                        'bid' : 'brand',
                                                        'mid' : 'model',
                                                        'pb' : 'select-brand',
                                                        'mb' : 'select-model',
                                                        'phid' : 'pval',
