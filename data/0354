function digi_search(){
  if(document.getElementById('digi_search_q').value==''||document.getElementById('digi_search_q').value=='查产品最新报价'){
         
         alert('请输入产品名称');
  }else{
         window.open('http://product.it.sohu.com/search/keyword='+document.getElementById('digi_search_q').value);
         }
}

