		try{
		function initTabs(){
		var vd=new Cookie(document,"SOHUHOMETAB",24);
		vd.load();
		vd.visit=(vd.visit==null)?parseInt(Math.random()*3+1):vd.visit;
		if(vd.visit!=0)hbTab(vd.visit);
		vd.visit++;
		vd.visit=(vd.visit>3)?1:vd.visit;
		vd.store();
		}
		initTabs();
