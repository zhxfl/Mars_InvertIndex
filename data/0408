<script language="javascript" type="text/javascript">

    var bookmarkLogger = {};
    amznJQ.available("jQuery", function () {
        (function ($) {
            bookmarkLogger["collectMetrics"] = function (metric, refTag) {
                $.ajax({
                    type : "POST",
                    url : "/gp/socialmedia/bookmark/backend/metrics.html/ref=" + refTag,
                    cache : false,
                    data : {
                        'metric' : metric
                    }
                });
            };
        })(jQuery);
    });
    function addToFavorites(url,name) {
        if(window.external && document.all) { // IE
            window.external.AddFavorite(url, name);
            try { bookmarkLogger.collectMetrics("ie", "gw_bm_ie"); } catch(e) {}
        } else {
            jQuery.AmazonPopover.displayPopover({
                  width: 400,
                  title:" 温馨提示",
                  literalContent : "<br/>请使用菜单栏或Ctrl+D收藏此站，以方便您轻松访问。<br/><br/>"
             });
            try {
                if(window.sidebar) { // Firefox
                    bookmarkLogger.collectMetrics("ff", "gw_bm_ff");
                } else {
                    bookmarkLogger.collectMetrics("ot", "gw_bm_ot");
                }
            } catch(e) {
                //ignore
            }
        }
    }
</script>





