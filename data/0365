                </a>
            </div>
            <div class="vote-tools clearfix">
                <label class="vote-tool-win vote-tool">
                    <h6>客队胜</h6>
                    <span><input type="checkbox" data-sp-key="0" data-sp-vale="<%=item.sp_current[0]%>"/> <%=item.sp_current[0]%></span>
                </label>
                <label class="vote-tool-lose vote-tool">
                    <h6>主队胜</h6>
                    <span><input type="checkbox" data-sp-key="3" data-sp-vale="<%=item.sp_current[3]%>"/> <%=item.sp_current[3]%></span>
                </label>
            </div>
        </div>
        <%
        }
        %>
    </div>
</script>

