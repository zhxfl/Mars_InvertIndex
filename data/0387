//ie6下通过脚本去设置样式
$('[data-hover]').live({
  mouseenter : function () {
    $(this).addClass($(this).data('hover'));
  },
  mouseleave : function () {
    $(this).removeClass($(this).data('hover'));
  }
});
</script>
