<script>
ZOL.use('*', function(ZT) {
    ZOL.topic = function(){
        
            /* 滚动组件 */
            var speed_topic=30;
            var scroll_begin_topic = document.getElementById("scroll_begin_topic");
            var scroll_end_topic = document.getElementById("scroll_end_topic");
            var scroll_div_topic = document.getElementById("scroll_div_topic");
            var scroll_cur_left = -1;
            scroll_end_topic.innerHTML = '';
            scroll_end_topic.innerHTML=scroll_begin_topic.innerHTML;
            function Marquee_Topic(){
                if(scroll_cur_left == scroll_div_topic.scrollLeft) {
                    scroll_div_topic.scrollLeft = 0;
                } else {
                    scroll_cur_left = scroll_div_topic.scrollLeft;
                    scroll_div_topic.scrollLeft++
                }
            }
            var MyMar2=setInterval(Marquee_Topic,speed_topic);
            scroll_div_topic.onmouseover=function() {clearInterval(MyMar2)} 
            scroll_div_topic.onmouseout=function() {MyMar2=setInterval(Marquee_Topic,speed_topic)} 
        
    }();
});

</script><!--AD620AD-->
