function initFixMenuBar(){
	var supportsTouches = ("createTouch" in document),
	startEvent = supportsTouches ? "click" : "mouseover",
	endEvent = supportsTouches ? "NELL" : "mouseout";
	var _menuL = [], _nM = null, _preM = null;
	var e=document.getElementById("fixMenuBar");
	if(e){
		var t=e.childNodes,n=null;
		for(var r in t)
			if(t[r].nodeType==1){
				n=t[r].childNodes;
					for(var i in n)
						n[i].nodeType==1&&n[i].className.indexOf("down-menu")>=0&&(
							_menuL.push(n[i]),
							n[i]["on"+startEvent]=function(ev){
								_preM = _nM;
								_nM = this;
								this.className.indexOf("show")<0&&(this.className+=" show");
							},
							n[i]["on"+endEvent]=function(){
								this.className.indexOf("show")>=0&&(this.className=this.className.replace(" show",""))
							}
						)
			}
		if(supportsTouches){
			document.body.ontouchstart=function(ev){
				var _sE = ev.srcElement;
				setTimeout(function(){
					if(!(_sE.parentNode == _nM || _sE.parentNode.parentNode == _nM) && _sE.parentNode.parentNode.id != "loginFrm")_nM.className=_nM.className.replace(" show","");
					if(!(_sE.parentNode == _preM || _sE.parentNode.parentNode == _preM) && _sE.parentNode.parentNode.id != "loginFrm")_preM.className=_preM.className.replace(" show","");
				},500);
			}
		}
	}
}
initFixMenuBar();
