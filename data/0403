    <script language="javascript" type="text/javascript">
        $(function(){
            if ($('#activity_time_footer').length > 0) {
            //var a = new Date(new Date().getFullYear(),new Date().getMonth(),new Date().getDate(),"23","59","59").getTime();
            var b = (new Date()).getTime();
            var a = parseInt( $('#activity_time_footer').val() ) * 1000;

            var e = function() {
                var c = (new Date()).getTime();
                var ls = a+b-c;
                if (ls > 0) {
                    var ld = parseInt(ls / 86400000).toString();
                    ld = ld.length > 1 ? ld : '0' + ld;
                    ls = ls % 86400000;
                    var lh = parseInt(ls / 3600000).toString();
                    lh = lh.length > 1 ? lh : '0' + lh;
                    ls = ls % 3600000;

                    var lm = parseInt(ls / 60000).toString();
                    lm = lm.length > 1 ? lm : '0' + lm;

                    var lms = parseInt((ls % 60000) / 100);
                    ls = parseInt(lms / 10);
                    var tms = lms - ls * 10;

                    ls = ls.toString().length > 1 ? ls : '0' + ls;

                    var html2 = '<span class="z">' + lm + '</span><span>分</span><span class="z">' + ls +'.' + tms + '</span><span>秒</span>';
                    $('#ms_timer').html(html2);
                } else {
                    $('#ms_timer').html('<span class="z">00</span><span>分</span><span class="z">' +'00.00'  + '</span><span>秒</span>');
                    $("#ms_timer").stopTime('timer');
                    $('.ms_box').hide();
                }
            };
            $("#ms_timer").everyTime(100, 'timer', e);
            }
        });

        $(function(){
            $('.ms_close').click(function(){
                $('.ms_box').hide();
            });

            var ms_box = function(){
                var left = 0,top = $(window).scrollTop() +$(window).height() -44 ;
                $('.ms_box').css({position:'absolute',left:0,top:top});
            };

            if($.browser.msie && $.browser.version == '6.0' ){
                $('.ms_box').everyTime(200, ms_box);
            }
        });

    </script>
    
    
    

