                        for ( var i = 0; i < len; i++ ) {
                            if ( i % 2 == 0 ){ %>
                            <div class="week-rank-odd tang-carousel-item week-rank-item ranklist_short tang-carousel-item-selected">
                            <% } else{ %>
                             <div class="tang-carousel-item week-rank-item ranklist_short">
                            <% } %>
                                <div id="<%=data[i]['name'] %>" class="mode-aside mode-list" tang-param="selectedIndex: 0;">
                                     <div class="mode-hd">
                                         <h3><%=RANK_LIST[data[i]['name']] %></h3>
                                     </div>
                                     <div class="mode-bd">
                                        <ol class="ranklist" static="s=1700&amp;tn=index_hao123&amp;bl=<%=data[i]['name'] %>&amp;stp=ti&amp;to=search" monkey="<%=data[i]['name'] %>" monkeyconfig="event=click&amp;amp;to=play">
                                        <% var list = data[i]['data']['videos'];
                                         for ( var j = 0 , list_len = list.length; j < list_len && j < 10; j++) {
                                                if ( j == 0) { %>
                                                    <li class="poster">
                                                         <span class="no no<%=j+1 %>">1</span>
                                                         <dl>
                                                            <dt>
                                                            <a href=<%=list[j]["intro_url"] %> target="_blank" static="no=1">
                                                                <img src=<%=list[j]["imgh_url"] %> />
                                                                <span class="s-title"><%=list[j]["title"] %></span>                                                               
                                                                <span class="v-all-bg">&nbsp;</span>
                                                                <% if (list[j]["rating"] != 0) { %>
                                                                <%  
                                                                    var t_rating =parseInt( list[j]['rating'] / 10 );
                                                                    var s_rating = list[j]['rating'] % 10;
                                                                    
                                                               %>
                                                                <span class="s-rating"><%=t_rating %><b>.</b><span><%=s_rating%></span></span>
                                                                <% } %>
                                                       
                                                            </a>
                                                            </dt>
                                                        </dl>
                                                     </li>
                                                <% } else { %>
                                                    <li class="list">
                                                         <span class="no no<%=j+1 %>"><%=j+1 %></span>
                                                            <a href=<%=list[j]["intro_url"] %> class="s_title" target="_blank" title=<%=list[j]["title"] %> static="no=<%=j+1 %>"><%=list[j]["title"] %></a>
                                                           <% if(list[j]['update'] !='') { %>
                                                            <span class="s-update"><%=list[j]['update'] %></span>
                                                            <% } else{
                                                            	var actor=''; 
                                                            	for ( var k = 0, actor_len = list[j]['show_actor'].length; k < actor_len; k++) {
                                                            		actor += list[j]['show_actor'][k]['name'] + ' ';
                                                            	}
                                                            	if (actor.length > 0 ) { 
                                                                    actor.length--;
                                                                }
                                                            	%>
                                                            	<span class="s-actor"><%=actor%></span>
                                                            <% } %>
                                                          
                                                     </li>
                                                <% }
                                            } 
                                           %>
                                        </ol>
                                    </div>
                                </div>
                            </div>
                            <% }
                        }
                    %>
                    </script>
                </div>
             </div>
