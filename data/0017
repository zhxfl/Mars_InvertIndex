    var bdshare = T.dom.q("bdshareBox")[0],
	firstbdshare = true;
    function showbdshare(x, y, data){
        var bx = x || 0,
            by = y || 0;

        if(data){
            bdshare.setAttribute("data", data);
        }

        bdshare.style.cssText = "display:block;position:absolute;left:"+bx+"px;top:"+by+"px;";
   	
	try{ 
	    if(firstbdshare && bds_config && T.dom.g("bdshare_l")){
                T.dom.g("bdshare_l").setAttribute("data","{'text':'"+ bds_config.bdText +"','title':'"+ bds_config.bdText +"'}");
	        firstbdshare = false;
    	    }
	}catch(e){}
    }

    function hidebdshare(){
        bdshare.setAttribute("data", "");
        T.dom.hide(bdshare);
    }

    if(T.dom.hasClass(bdshare, "bdshareBox_r")){
       window.onresize = function(){
           bdshare.style.left = T.dom.getPosition("main").left + 959 + "px";
       } 
    }
</script>

