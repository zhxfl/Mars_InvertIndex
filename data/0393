//明星专栏
(function(){
	$('#star dl dt').each(function(i){
		$(this).css('background','url(http://img7.imglafaso.com/images/newhome/starimg.jpg) 0 ' + i * -61 +'px no-repeat').parent().attr('i',i);
	});

	var starHoverT;
	$('#star dl').live('mouseenter',function(){
		clearTimeout(starHoverT);
		var n = $(this).index(),h = $(this).attr('h'),i=$(this).attr('i');
		starHoverT = setTimeout(function(){
			if($('#starHover').css('width') != '0px'){
				var curY = $('#starHover').css('top').replace('px','');
				var oneY = curY > n * 87 ? 24 : 10,
				twoY = oneY == 10 ? 24 : 10;
				$('#starHover').stop().animate({'top':n * 87 - oneY},100).animate({'top':n * 87 - twoY},100).animate({'top':n * 87 - 18},50);
			}else{
				$('#starHover').css({'top':n * 87 - 18}).html('<b></b>');
				$('#starHover').animate({'width':226},200);
			}
			$('#starHover b').css('margin-top',i * -127).parent().attr('href',h);
		},200);
	});
	
	$('#star').mouseleave(function(){
		clearTimeout(starHoverT);
		$('#starHover').animate({'width':0},200);
	});

	var starAnim = false;
	$('#star .next').click(function(){
		if(starAnim) return false;
		starAnim = true;
		$('#star dl:first').stop().slideUp(200,function(){starAnim = false;$('#star dl:last').after($(this));});
		$('#star dl').eq(7).slideDown(200);
	}).mouseenter(function(){
		$('#star').trigger('mouseleave');
	});

	var autoTab;
	$('#star').mouseover(function(){
		clearInterval(autoTab);
	}).mouseout(function(){
		clearInterval(autoTab);
		autoTab = setInterval(function(){
			$('#star dl:lt(7)').stop().slideUp(200,function(){starAnim = false;$('#star dl:last').after($(this));});
			$('#star dl:not(:animated):lt(7)').css('height','').slideDown(200);		},8000);
	}).trigger('mouseout');

})();

//轮播器
var Slide = function(o){
	this.dom = {img:'#slide div',btn:'#slide b',cls:'on',e:'mouseenter',t:5000};
	this.timeout;
	this.index = 0;
	$.extend(this.dom,o);
	this.init();
	this.automatic();
}

Slide.prototype = {
constructor:Slide,
			init:function(){
				var _self = this,_nowshow = $(_self.dom.img+':first');
				_nowshow.s = true;
				_nowshow.css('z-index',11);
				$(_self.dom.btn).bind(_self.dom.e,function(){
						if($(this).hasClass(_self.dom.cls)){return false;}
						$(this).siblings().removeClass(_self.dom.cls);
						var i = $(this).addClass(_self.dom.cls).index();
						_self.index = i;
						var _showImg = $(_self.dom.img).eq(_self.index);
						_self.imgShow(_showImg);
						_nowshow.css('z-index',11);
						_showImg.stop().css({'z-index':10,'opacity':1}).show();
						_nowshow.fadeOut(300,function(){
							$(this).css('z-index',1);
							});
						_nowshow = _showImg;
						});
				$(_self.dom.img).parent().hover(function(e){
					clearTimeout(_self.timeout);
				},function(){
					_self.automatic();
				});
			},
imgShow:function(showImg){
			showImg.attr('i') && showImg.css('background','url('+ showImg.attr('i') +') no-repeat center').removeAttr('i');
			showImg.attr('c') && showImg.css('background-color',showImg.attr('c')).removeAttr('c');
		},
automatic:function(){
			  var _self = this;
			  if(_self.index >= $(_self.dom.btn).length - 1){_self.index = -1;}
			  clearTimeout(_self.timeout);
			  _self.timeout = setTimeout(function(){
					  $(_self.dom.btn).eq(_self.index + 1).trigger(_self.dom.e);
					  _self.automatic();
					  },_self.dom.t);
		  }
}

new Slide();


//公告
var _noticeTabSwitch
var noticeTab = function(i){
	var _firstDD = $('#notice dd:first');
	if(!_noticeTabSwitch){
		if (!i){
			$('#notice').hover(function(){_noticeTabSwitch = !_noticeTabSwitch});
			_firstDD.addClass('on');
			i = 1;
		}else{
			$('#notice dd.on').animate({
					'height':'0',
					'top':'23px'
					},100,function(){
					if(i == $('#notice dd').length){i = 0};
					$('#notice dd').removeClass('on').eq(i++).addClass('on').animate({
						'height':'13px',
						'top':'17px'
						},100);
					});
		}
	}
	setTimeout(function(){noticeTab(i)},5000);
}
noticeTab();

	//图片加载 首页专用
		function imgLoad(o){// imgload
					var _s = o || 'img',n = n || 0,h = function(){var d=document,y=(navigator.userAgent.toLowerCase().match(/iPad/i)=="ipad")?window.pageYOffset:Math.max(d.documentElement.scrollTop,d.body.scrollTop);return d.documentElement.clientHeight+y-n;},tim,_time = 0,
					l = function(){
						var hg = h();
						$(_s).each(function(){
					if($(this).is(':hidden')){return true}	
					var _s2 = $(this).attr('src2'),t = $(this).offset().top;
							if(_s2 && t < hg && t > hg - 1000){$(this).attr("src",_s2).removeAttr('src2')};
						})};
						l();
						if(!!window.ActiveXObject && !window.XMLHttpRequest){_time = 600}
						$(window).scroll(function(){
							clearTimeout(tim);
							tim = setTimeout(function(){l()},_time);
						});
				}

imgLoad();

$.widthReset(function(){
	$('#tedList').addClass('ted-min');
	$('#minnav').width(984).height(60).hasClass('abs') && $('#minnav').css({'width':'1000px','margin-left':'-508px'});
	$('body').addClass('Cmin');
},function(){
	$('#tedList').removeClass('ted-min');
	$('#minnav').width(1184).height(30).hasClass('abs') && $('#minnav').css({'width':'1200px','margin-left':'-608px'});
	$('body').removeClass('Cmin');
});

$(window).load(function(){
 



		var j = $('#minNavL'),tnav=$('#minNavT'),t = $('#tedList').offset().top+33,s = false,showT;
		$(window).scroll(function() {
			if ($(this).scrollTop() > 500) {
				$("#scroll_Top").css('visibility','visible');
			}else{
				$("#scroll_Top").css('visibility','hidden');
			};


			var nu = $(this).scrollTop();

			if(!s && nu > t && $('body').outerHeight() - ($(window).scrollTop() + $(window).outerHeight()) > 1400){
				j.addClass('abs').removeClass('sie');
				tnav.addClass('abs');//.prev().css('margin-bottom',j.outerHeight());
				j.prepend('<img src="http://img4.imglafaso.com/images/newhome/ted_icon9.png"/>');
				tnav.find('a img').attr('src','http://img4.imglafaso.com/images/newhome/ted_icon10.png').css({'width':'1007px','height':'55px','left':'-7px'});
				tnav.css('padding','55px 0 0');
				if($('body').hasClass('Cmin')) $('#tedList').css('padding-top',tnav.outerHeight());
				s = true;
			}
			
				if($('body').outerHeight() - ($(window).scrollTop() + $(window).outerHeight()) < 500 + $('#minNavL').outerHeight() && j.hasClass('abs')){
					j.css({'position':'absolute','top':$('body').outerHeight() - 500 - j.outerHeight()});
				}else if(parseInt(j.css('top')) == $('body').outerHeight() - 500 - j.outerHeight()){
					j.removeAttr('style');
				}

			if(s && nu <=t){
				j.removeClass('abs').addClass('sie');
				tnav.removeClass('abs').addClass('sie');//.prev().css('margin-bottom','0');
				j.find('img').remove();
				tnav.find('a img').attr('src','http://img4.imglafaso.com/images/newhome/ted_icon7.png').removeAttr('style');
				tnav.removeAttr('style');
				 $('#tedList').css('padding','');
				s = false;
			}
		});

		//筛选
$('#minNavL a,#minNavT div a').click(function(){
	if(!$(this).hasClass('on') && $(this).attr('class')){
	   $('#tedList .banner').hide();
		var cl = '.' + $(this).attr('class');
		$('#minNavL a,#minNavT div a').removeClass('on').find('i').remove();
		$('#minNavL,#minNavT div').find(cl).addClass('on');
		$('#minNavL').find(cl).append('<i></i>');
		$('.contR-main div').hide().filter(cl).each(function(i){$(this).find('.num').text(i + 1)}).show();
		if(cl == '.zsgz'){
			$('#tedList .page').show()
		}else{
			$('#tedList .page').hide()
		}
		setTimeout(function(){$('#minNavL').removeAttr('style');$(window).scrollTop($(window).scrollTop() + 1);},100)
	}
});

//导航展开
$('#minNavT p b').click(function(){
if($(this).attr('style')){
    $(this).removeAttr('style').parent().prev().css('height','32px');
}else{
    $(this).css('background-position','0 -43px').parent().prev().css('height','auto');
}
});

//滑过效果
$('#tedList .contR-main div').hover(function(){
    $(this).css('border-color','#963c00');
},function(){
    $(this).css('border-color','#E3E3E3');
});

$('#tedList .page').hover(function(){
    $(this).css('border-color','#fc0').find('a').css('color','#000');
},function(){
    $(this).css('border-color','#ddd').find('a').css('color','#999');
});


			//加入购物车
		$('#tedList a.join').click(function(){
			var toObj = $(this);
			var sid = toObj.attr("sid");	
			var pid = toObj.attr("pid");	
			if(!sid) return true;
_tag.dcsMultiTrack('wt.ct','button','wt.s_pg','meili','wt.pid',sid,'wt.nu','1');
			$.getJSON("http://shopping.lefeng.com/cart/ajaxUpsellcart.jsp?callback=?",
				{productId:pid,skuId:sid,quantity:1,itemType:0,process:"add",skuExtraPriceId:0},
				function(json){
					if(json.errorInfo != ''){
						alert(json.errorInfo)
					}else{            		
						toObj.text('已加入购物车').attr('class','success');
					}
				}
			);
			return false;
		});


});

    $(function() {
		$("#zxfk").click(function(){
			if($('#onlineSurvey').is(':visible')){$('#onlineSurvey').hide();return false;}
			$('#onlineSurvey strong').remove();
			$('#onlineSurvey textarea').val('您的声音对我们很重要哟~').css('color','#999');
			$('#Scontact').val('请留下您的联系方式（选填）').css('color','#999');
			$('#Sgood').attr('checked',true);
			$('#onlineSurvey').show();
		});

		$("#Sclose").click(function(){
			$("#onlineSurvey").hide();	
		});

		$("#Ssubmit").click(function(){
			if($('#onlineSurvey textarea').val() == '您的声音对我们很重要哟~' || $('#onlineSurvey textarea').val() == ''){
				$('#onlineSurvey textarea').focus();
			}else{
				var cont = $('#onlineSurvey textarea').val(),
				gob = $('input[name="good_or_bad"]:checked').val(),
				contact = $('#Scontact').val() == '请留下您的联系方式（选填）' ? '' : $('#Scontact').val();
				$.getJSON('http://survey.lefeng.com/saveFankui.jsp?callback=?',{
					content:cont,
					good_or_bad:gob,
					contact:contact,
					url:location.href
				});
			$("#onlineSurvey").append('<strong style="color:#6baa00;">提交成功，感谢您对乐蜂网的支持！</strong>');
			setTimeout(function(){$("#onlineSurvey").slideUp(200)},1000);

			}
		});
		
		$('#onlineSurvey textarea').focus(function(){$(this).val() == "您的声音对我们很重要哟~" && $(this).val('').css("color","#333")}).blur(function(){$(this).val() == "" && $(this).val('您的声音对我们很重要哟~').css("color","#999")});
		
		$('#Scontact').focus(function(){$(this).val() == "请留下您的联系方式（选填）" && $(this).val('').css("color","#333")}).blur(function(){$(this).val() == "" && $(this).val('请留下您的联系方式（选填）').css("color","#999")});
		
	});


$('#tedList > a').append('<div class="shadowB"></div><div class="shadowR"></div>');

$('#tuan .tuan-list dl').mouseenter(function(){
	$('#tuan .tuan-list dd').hide();
	$('#tuan .tuan-list dt').show();
	$(this).find('dt').hide().next().show();
});

