!(getCookie("isshowcxFloat") * 1) && $(".cxTipBox").addClass("cxSmallTipBox");

$("#cxClose").click(function(){
	$(".cxTipBox").addClass("cxSmallTipBox");
	setCookie("isshowcxFloat",0);
    return false;
});

