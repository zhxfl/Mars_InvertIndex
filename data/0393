function IsPC()
{
var userAgentInfo = navigator.userAgent;
var Agents = new Array("Android", "iPhone", "SymbianOS", "Windows Phone", "iPod","BlackBerry","MeeGo","J2ME");
var flag = true;
for (var v = 0; v < Agents.length; v++) {
if (userAgentInfo.indexOf(Agents[v]) > 0) { flag = false; break; }
}
return flag;
}

if(!IsPC() && location.href.indexOf('source=wap') < 1){
location.href='http://m.lefeng.com';
}

if(navigator.userAgent.indexOf('iPad') > 0 && location.href.indexOf('source=ipad') < 1){
location.href='http://js.imglafaso.com/js/wap/html/20130812/';
}
