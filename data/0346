<script type="text/javascript">var sugapi="http://service.baidu.com";</script>
<script type="text/javascript" src="/static/js/sug_.js"></script>
<script type="text/javascript" src="../static/js/commonTousu.js"></script>
<script type="text/javascript">
var chkForm = {
	vaild: {
		link: /^((http:\/\/(baike|bk)\.baidu\.com)|((baike|bk)\.baidu\.com))/i,
		time:/^(\d{2}|\d{4})[-\/\.年]{1}\d{1,2}[-\/\.月]{1}\d{1,2}(日)?(( \d{1,2}\:\d{1,2})|( \d{1,2}\:\d{1,2}\:\d{1,2}))$/
	},
	msg: {
		title_empty: vaildForm.msg["title_empty"],
		title_long: vaildForm.msg["title_long"],
		link_empty: vaildForm.msg["link_empty"],
		link_err:"您输入的不是百度百科地址，请重新输入",
		link_exist: vaildForm.msg["link_exist"],
		extra_empty:"请输入词条名",
		ID_err: vaildForm.msg["ID_err"],
        ID_empty: vaildForm.msg["ID_empty"],
        ID_exist: vaildForm.msg["ID_exist"],
        ID_long: vaildForm.msg["ID_long"],
		content_empty: vaildForm.msg["content_empty"],
	    content_long: vaildForm.msg["content_long"],
	    time_empty: "请输入提交时间",
	    time_err: "时间格式有误"
	},

	getLinkCfg: function(formIdNum){
		var linkChkCbk = function(linksWrapId){
			return function(obj,rlt){
				var links = baidu("#" + linksWrapId).find("input.formlink");
				if(!rlt){
					for(var i = 0, l = links.length; i < l; i ++){
			 			var empty = vaildForm.isEmpty(links[i]);
			 			if(empty && (links[i] != obj)){
			 				vaildForm.setChkMsg(links[i],"");  //清空empty错误信息
			 			}
			 		}
			 		return false;
				}
				var formMsg = chkForm.msg;
				if(vaildForm.isEmpty(obj)){
			 		var isEmpty = true;
			 		for(var i = 0, l = links.length; i < l; i ++){
			 			var empty = vaildForm.isEmpty(links[i]);
			 			if(empty){
			 				vaildForm.setChkMsg(links[i],"");  //清空empty错误信息
			 			}
			 			isEmpty = isEmpty && empty;
			 		}

			 		if(isEmpty){
			 			vaildForm.setChkMsg(obj, formMsg["link_empty"]);
			 			return false;
			 		}
			 	} else {
			 		//检查是否重复
			 		var val = obj.value;
			 		if(!vaildForm.http(val)){
			 			obj.value = "http://" + val;
			 		}
			 		val = baidu.string(val).trim();
			 		var isRepeat = false;
			 		for(var i = 0, l = links.length; i < l; i ++){
			 			if(links[i] != obj && (baidu.string(links[i].value).trim() == val)){
			 				isRepeat = true;
			 				break;
			 			}
			 		}

			 		if(isRepeat){
			 			vaildForm.setChkMsg(obj, formMsg["link_exist"]);
			 			return false;
			 		}
			 	}
			 	return true;
			};
		};
		var config = {
			"regExp": chkForm.vaild.link,
			"errMsg" : chkForm.msg["link_err"],
			"noSubmit":true,
			"callback": linkChkCbk("form" + formIdNum + "_link")
		};
		return config;
	},
	bindAddLink: function(formIdNum, formO, linkCfg){
		baidu("#form"+ formIdNum +"_link_add").click(function(){
			var callback = function(idAry){
				var fields = {};
				for(var i = 0, l = idAry.length; i < l; i ++){
					fields["#" + idAry[i]] = linkCfg;
				};
				formO.vaildForm({"fields" : fields});
			};
			tousu.addLink("form"+ formIdNum +"_link",3,12, callback);
		});
	},
	submitCbk: function(linksWrapId,rlt){
		var formMsg = chkForm.msg;
		var links = baidu("#" + linksWrapId).find("input.formlink");
		var linkReg = chkForm.vaild.link;
		var urls = [];
		var tempLinksId = [];
		var hasErr = false;
		var result = true;
			for(var i = 0; i < links.length; i++){
				var link = links.eq(i);
				var url = baidu.string(link.val()).trim();
				vaildForm.setChkMsg(link[0],"");  //清空提示
				if(url == ""){ continue; }
				var tempErrMsg = null;
				if(linkReg.test(url)){
					url = url.replace(/\t/g,"");
					var tempHasRepeat = false;
					for(var j = 0, l = urls.length; j < l; j ++){
						if(url == urls[j]){
							tempHasRepeat = true;
							break;
						}
					}

					if(!tempHasRepeat){
						urls.push(url);
						tempLinksId.push(link.attr("id"));
						tempErrMsg = null;
					} else {
						hasErr = true;
						tempErrMsg = formMsg.link_exist;
					}

				} else {
					hasErr = true;
					tempErrMsg = formMsg.link_err;
				}
				vaildForm.setChkMsg(link[0],tempErrMsg);  //显示正确/错误
			}

			if(hasErr){
				result = false;
			} else if(urls.length <= 0 && (!hasErr)){
				vaildForm.setChkMsg(links[0],formMsg.link_empty);
				result = false;
			} else {
				tousu["tempLinksId"] = tempLinksId.concat();
				urls = urls.join("\t");
				baidu("#" + linksWrapId + "s").val(urls);
			}
			result = result && rlt;
			return result;
	},
	setCurrentTime: function(ipt){
	    var today = baidu.date.format(new Date(),'yyyy-MM-dd');
	    ipt.value = today;
	},
	changeBtnSubmit: function(formIdNum, isFinish){
		var self = this;
		var btnSubmit = baidu("#form" + formIdNum + "_btnSubmit");
		
		if(isFinish){
			btnSubmit.removeClass("btn_off");
			btnSubmit.val("提交");
			btnSubmit.removeAttr("submiting");
		} else {

			if(btnSubmit.attr("submiting") == "1"){ return false;}
			btnSubmit.addClass("btn_off");
			btnSubmit.val("正在提交");
			btnSubmit.attr("submiting", "1");
		}
		return true;
	}
};

var form1 = {
	formO : baidu("#tousu_form1"),
	init: function(){
		this.bindCheck();
		chkForm.bindAddLink("1", this.formO, this.linkCfg);
	},
	bindCheck: function(){
		var formO = this.formO;
		var formMsg = chkForm.msg;
		var linkCfg = this.linkCfg = chkForm.getLinkCfg("1");
		formO.vaildForm({
			fields:{
				"#form1_title": {
					required:true,
			        regExp:"len",
			        emptyMsg: formMsg["title_empty"],
			        errMsg: formMsg["title_long"]
				},
				"#form1_link1": linkCfg,
				"#form1_link2": linkCfg,
				"#form1_link3": linkCfg,
				"#form1_ids":{
			        regExp:"validId",
			        emptyMsg: formMsg["ID_empty"],
			        errMsg: formMsg["ID_err"],
			        longMsg: formMsg["ID_long"]
				},
				"#form1_content":{
					required:true,
			        regExp:"len",
			        emptyMsg: formMsg["content_empty"],
			        errMsg: formMsg["content_long"]
				}
			},
			submitCbk: function(rlt){
				var chkRlt = chkForm.submitCbk("form1_link", rlt);
				if(chkRlt){
					chkRlt = chkForm.changeBtnSubmit(1);
				}
				return chkRlt;
			}
		});
	}
};

var form2 = {
	formO : baidu("#tousu_form2"),
	init: function(){
		this.bindCheck();
		var iptTime = baidu("#form2_time")[0];
		/*
		if(vaildForm.isEmpty(iptTime)){
			chkForm.setCurrentTime(iptTime);
		}
		var datepicker = new magic.setup.datePicker('form2_time',{
                'format': 'yyyy-MM-dd HH:mm',
                'calendarOptions': {
                    'timer': {
                        enable: true
                    }
                }
            });

	*/
	},
	bindCheck: function(){
		var self = this;
		var formO = this.formO;
		var formMsg = chkForm.msg;
		formO.vaildForm({
			fields:{
				"#form2_title": {
					required:true,
			        regExp:"len",
			        emptyMsg: formMsg["title_empty"],
			        errMsg: formMsg["title_long"]
				},
				"#form2_ids":{
					required:true,
			        regExp:"validId",
			        emptyMsg: formMsg["ID_empty"],
			        errMsg: formMsg["ID_err"],
			        longMsg: formMsg["ID_long"]
				},
				"#form2_extra":{
					required:true,
			        regExp:"isEmpty",
			        emptyMsg: formMsg["extra_empty"]
				},
				"#form2_time":{
					required:true,
			        regExp: chkForm.vaild.time,
			        emptyMsg: formMsg["time_empty"],
			        errMsg: formMsg["time_err"]
				},
				"#form2_content":{
					required:true,
			        regExp:"len",
			        emptyMsg: formMsg["content_empty"],
			        errMsg: formMsg["content_long"]
				}
			},
			submitCbk: function(rlt){
				if(rlt){
					rlt = chkForm.changeBtnSubmit(2);
				}
				return rlt;
			}
		});
	}
};

