      // 		o.style.color = "#000";
      // 		if(o.value == $(o).data('default')){ o.value = '';}
      // 	}
      // 	if(e === 'b' && !o.value){ o.style.color = "#999"; o.style.fontSize = "14px"; o.value = $(o).data('default'); }
      // }
