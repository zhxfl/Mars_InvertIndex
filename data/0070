        "tplContent":"<div><div class=\x22mod-relation clearfix\x22> <h2 class=\x22headline yahei\x22>关注\/粉丝<\/h2> <div class=\x22relation-detail clearfix\x22> <dl> <dd>  <span class=\x22yahei rl-count-follows relation-span\x22>0<\/span>   <dt>关注 <\/dl> <div class=relation-split><\/div> <dl> <dd>  <a href=\x22#\x22 class=\x22relation-link rl-count-fans yahei js-friendlist\x22 data-part=fans>190<\/a>   <dt>粉丝 <\/dl> <\/div> <div class=js-relationBox data-from=\x22tieba\x22> <div class=\x22show-follow clearfix\x22  style=display:none > <div class=follow-content>已关注<\/div><a class=cancel href=\x22#\x22>取消<\/a> <\/div> <div class=relation-button  > <a href=\x22#\x22 class=add-follow><\/a> <\/div><\/div><\/div><\/div>", 
        "options": { 
            "init": function(){
                var relationCount = {
                    fans : 190,
                    follows: 0 
                };
                this.router.updateGlobal({"tiebaRelationCount":relationCount});
                // new code
                var options = {};
                options["events"]={
                    "change:count": (function(evt){
                        if(evt.channel === "tieba"){
                            this.dispatchEvent("change:count", evt);    
                        }
                    }).bind(this)
                };
                // end new code
                this.router.updateGlobal({"tiebaRelationCount":relationCount});
                // new code
                this.router.updateCurrentUserData({"channel": "tieba", "relation":{"count":relationCount}});
                this.router.bindCurrentUserEvent(this, options);
                this.router.bindLoginUserEvent(this, {"events": {"update:friendlist": (function(evt){
                    if(evt.channel === "tieba" && evt.data.list && evt.data.list[0].portrait === this.router.global.page_userInfo.portrait){
                        this.dispatchEvent("update:relationbtn", {"data":{"opt": evt.opt}});
                    }
                }).bind(this)}});
                                // end new code
            }
        }
    });
</script>