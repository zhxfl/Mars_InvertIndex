     <script type="text/javascript">
function SliderTimer() {
    this.scroll = setInterval(function () {
        var dvSlider = $("div[class='focusMap']");
        var index = dvSlider.find("a").index(dvSlider.find("a:visible"));
        index = index + 1;
        if (index == dvSlider.find("a").length)
            index = 0;
        dvSlider.find("li").eq(index).trigger('click');
    } .bind(this), 10000);
}
function Slider() {
    var dvSlider = $("div[class='focusMap']");
    var btnList = dvSlider.find("li");
    btnList.bind("click", function (evt) {
        var element = Event.element(evt);
        btnList.removeClass("action");
        element.addClass("action");
        dvSlider.find("a:visible").fadeOut(700);
        var index = btnList.index(element);
        dvSlider.find("a:eq(" + index + ")").fadeIn(1000);
 evt.stopPropagation();
    } .bind(this));
    SliderTimer();
}
function ScrollGroupOn() {
    var step = 275;
    var dvGroupOn = $("div[class='Promotions']");
    var leftBtn = dvGroupOn.find("a[method='leftBut']");
    var rightBtn = dvGroupOn.find("a[method='rightBut']");
 
    var left = dvGroupOn.find("div[class='groupon']").css("margin-left").replace("px", "");
    if (parseInt(left) == 0) {
        leftBtn.addClass("leftBut_no");
    }
    rightBtn.bind("click", function (evt) {
        var dvScroll = dvGroupOn.find("div[class='groupon']");
        var right = dvScroll.css("margin-left").replace("px", "");
        var item = dvScroll.find("dl");
        if (item.length > 2 && (parseInt(right) - step >= (2 - item.length) * step)) {
  leftBtn.removeClass("leftBut_no");
  if (parseInt(right) - step == (2 - item.length) * step)
  {
   rightBtn.addClass("rightBut_no");
  }
  dvScroll.animate({ marginLeft: (parseInt(right) - step) + "px" });
        }
    } .bind(this));
    leftBtn.bind("click", function (evt) {
        var dvScroll = dvGroupOn.find("div[class='groupon']");
        var left = dvScroll.css("margin-left").replace("px", "");
        var item = dvScroll.find("dl");
        if (item.length > 2 && parseInt(left) < 0 && (parseInt(left) + step <= (item.length - 2) * step)) {
  rightBtn.removeClass("rightBut_no");
  if (parseInt(left) + step == 0)
  {
   leftBtn.addClass("leftBut_no");
  }
  var leftLength = parseInt(left) + step;
  if (leftLength > 0) {
   leftLength = 0;
  }
  dvScroll.animate({ marginLeft: leftLength + "px" });
        }
    } .bind(this));
}
function InitSeo() {
    var seo = $("div[class='bottom_nav']");
    var detail = $("div[class='bottom_con']");
    detail.hide();
    seo.bind("click", function (evt) {
        var element = Event.element(evt);
        if (element.is("a") || element.is("li")) {
            if (element.is("a")) element = element.parents("li");
            seo.find("li").removeClass("action");
            element.addClass("action");
            var index = seo.find("li").index(element);
            detail.hide();
            detail.eq(index).show();
        }
    } .bind(this));
}
function clickEmailSubmit() {
 var email = $("#email").val();
 if (validator.valid(email, "notEmpty & email") == false) {
  alert("请输入订阅邮箱或邮箱格式不正确！");
  return false;
 }
 elongAjax.callBack("http://hotel.elong.com/isajax/EmailSubscribe/getorignalemail", {}, function (res) {
  var isUpdateEmail = false;
  if (res.isLogin && res.orginalEmail != email) {
   if (confirm("与注册email不一致，问是否需要更新注册email")) {
    isUpdateEmail = true;
   }
  }
  var requestParams = { email: email, subscribeTypes: [1,2,3,4,6,8], isUpdateEmail: isUpdateEmail };
  elongAjax.callBack("http://hotel.elong.com/isajax/EmailSubscribe/Subscribe", requestParams, function (result) {
   alert(result.msg);
  }, false, "GET", null, "jsonp");
 }, false, "GET", null, "jsonp");
}
function InitHomePage() {
    Slider();
    ScrollGroupOn();
    InitSeo();
}
InitHomePage();
</script>
