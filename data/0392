<script type="text/javascript">
(function(){
var JExtend = pc.getElem('#JExtend');
var animateHeight = ''
pc.addEvent(JExtend,'click',function(){
var arrow = pc.getElems('em',this)[0];
var JPlate = pc.getElem('#JPlate');
if(pc.hasClass(JExtend,'extended')){
JPlate.style.display = 'none';
pc.removeClass(JExtend,'extended')
}else{
JPlate.style.display = 'block';
pc.addClass(JExtend,'extended')
}
})
var eval_fn = '',ids = ['hqBlock','notebook','mobile','diy','camera','software','digital','office','pie'],timer,current,scrolltop;
window._doScroll = function(el){
if(typeof current !== 'undefined'){
document.getElementById('cl_'+current).className = document.getElementById('cl_'+current).className.replace(/\s*\bcur\s*/,'');
if(el === 'last'){
return;
}
}
document.getElementById('cl_'+el).className = 'cur';
current = el;
} 
window.onload = function(){
var target,next,target_top,next_top,upperLimit,lowerLimit;
scrolltop = document.body.scrollTop || document.documentElement.scrollTop;
for(var i = 0,l = ids.length;i<l;i++){
typeof target === 'undefined' && (target = document.getElementById(ids[0]),target_top = target.getBoundingClientRect().top - 2 + scrolltop);
try{
next = document.getElementById(ids[i+1]);
next_top = next.getBoundingClientRect().top - 2 + scrolltop;
lowerLimit = (next_top - target_top) / 2 + target_top;
}catch(e){
lowerLimit = target.parentNode.parentNode.offsetHeight / 2 + target_top;
}
typeof upperLimit === 'undefined' && (upperLimit = target_top - 200); 
if(i === l-1){
eval_fn += 'if(d >= '+upperLimit+' && d < '+lowerLimit+'){_doScroll.call(this,"'+ids[i]+'")}';
eval_fn += 'if(d >= '+lowerLimit+'){_doScroll.call(this,"last")}';
}else{
eval_fn += 'if(d >= '+upperLimit+' && d < '+lowerLimit+'){_doScroll.call(this,"'+ids[i]+'")}';
}
target = next;
target_top = next_top;
upperLimit = lowerLimit;
}
window.fn = new Function('d',eval_fn);
window.onscroll = function(){
var d = document.body.scrollTop || document.documentElement.scrollTop;
clearTimeout(timer);
timer = setTimeout(function(){
var ot = document.getElementById('JOffset').offsetTop;
var guideTag = document.getElementById('guideTag');
if((d + document.documentElement.clientHeight) > ot){
guideTag.style.display = 'block';
}else{
guideTag.style.display = 'none';
typeof current !== 'undefined' && (document.getElementById('cl_'+current).className = document.getElementById('cl_'+current).className.replace(/\s*\bcur\s*/,''),current = undefined)
}
// global scope
fn.call(this,d);
},10)
}
}
})()
</script>
<!--右侧浮动块end-->
