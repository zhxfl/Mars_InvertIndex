loadDefaultData();
attachEvts();
function attachEvts() {
var len = tocs.length;
for (var i = 1; i < len; i++) {
tocs[i].onclick = function(e) {
e = e || window.event;
var city = this.innerHTML;
this.parentNode.parentNode.parentNode.parentNode.parentNode.parentNode.className = 'pcPop';
setCookie('PClocation', city);
switchTo(city);
if (e.stopPropagation) e.stopPropagation();
else e.cancelBubble = true;
return false;
}
}
}
function loadDefaultData() {
window.PClocation = unescape(getCookie("PClocation"));
var city;
needJS(window.PClocation, 'http://whois.pconline.com.cn/jsFunction.jsp?callback=IPcallback',
function() {
for (var i = 0; i < specialMap.length; i++) {
if (window.PClocation.indexOf(specialMap[i][1]) != -1) {
city = specialMap[i][0];
current = specialMap[i];
break;
}
}
//查找城市
if (!city) {
for (var i = 0; i < cityMap.length; i++) {
var index = window.PClocation.indexOf(cityMap[i][0]);
if (index != -1) {
city = cityMap[i][0];
current = cityMap[i];
break;
}
}
}
//匹配相关城市
if (!city) {
for (var key in aliasMap) {
var relaMaps = aliasMap[key].split(','),
relaLen = relaMaps.length;
for (var k = 0; k < relaLen; k++) {
var relaMap = relaMaps[k];
if (window.PClocation.indexOf(relaMap) != -1) {
city = key;
for (var i = 0; i < cityMap.length; i++) {
if(cityMap[i][0] == key){
current = cityMap[i];
break;
}
};
break;
}
};
}
}
//如果没有对应分站的话，默认出北京
if (!city) {
city = '北京';
current = cityMap[2];
}
var curAdr = (city == '全国') ? 'market': current[2];
$$("curAdress").getElementsByTagName('span')[0].innerHTML = '<a href="http://' + curAdr + '.pconline.com.cn" target="_blank">' + city + '</a>';
switchTo(city || '全国');
});
}
function switchTo(city) {
var ab, domain;
for (var i = 0; i <= cityMap.length; i++) {
if (city.indexOf(cityMap[i][0]) != -1) {
current = cityMap[i];
break;
}
}
domain = (city == '全国') ? 'market': current[2];
head.innerHTML = '<a href="http://' + domain + '.pconline.com.cn" target="_blank">' + city + '行情</a>';
loadData();
}
function loadData() {
var ab = current[1],
domain = current[2];
if (datas[ab]) {
wrap.innerHTML = datas[ab];
return;
}
//head.text = current[0] + '行情';
/*head.innerHTML = '加载中...';*/
var callbackName = 'callbackIndex' + Math.ceil(Math.random() * 1000);
needJS(0, 'http://www.pconline.com.cn/autopublish/ipdatajs/home_ip_data2012/' + ab + '.js?callback=' + callbackName);
window[callbackName] = function(data) {
var str = '',
len = data.length < 25 ? data.length: 25;
str += '<div class="bigTitC">';
var cOrder = speOrder[ab] || order;
for (var i = 0; i < cOrder.length; i++) {
var d = data[cOrder[i]];
if (!d) continue;
if (i == 0 || i == 7 || i == 14 || i == 19) str += '<a href="'+ d.url +'" title="'+d.title+'">'+d.title+'</a></div><ul class="expTxtA noMdTxt f14">';
else{
str += '<li><i class="iSub"><a href="http://' + domain + '.pconline.com.cn/" target="_blank">' + typeMap[d.type] + '</a> |</i> <a href="'+ d.url +'" title="'+d.title+'">'+d.title+'</a></li>';
}
if (i == 6 || i == 13 || i == 18) str += '</ul><div class="bigTitC">';
}
str += '</ul>'
datas[ab] = str;
//head.innerHTML = head.text;
//除异步加载的无序性
if (ab == current[1]) loadData(ab);
}
}
//IP识别回调函数	
window.IPcallback = function(city) {
window.PClocation = city;
setCookie('PClocation', escape(city));
}
}
</script>
