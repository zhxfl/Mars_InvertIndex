<script type="text/javascript">
(function(){
//获取行情
var aList = pc.getElems('#tweTab a');
var areaList = [];
for (var i = 0; i < aList.length; i++) {
if (aList[i].getAttribute('area') !== null) {
areaList.push(aList[i]);
}
}
window.areaTabCallback = function(data) {
var area = data.area,
html = data.html;
var tab = document.getElementById('hq-tab-' + area);
if (!tab) {
return;
}
tab.innerHTML = html;
tab.setAttribute('loaded', 'true');
}
/*全国行情*/
new pc.tab({
target: pc.getElems('#tweTab .contentdiv'),
control:  pc.getElems('#tweTab .hqToc'),
link : true,
onchange : function(){
var curindex = this.curPage;
var area = areaList[curindex].getAttribute('area'),
areaName = areaList[curindex].innerHTML;
if (!area || document.getElementById('hq-tab-' + area) === null) {
return;
}
var tab = document.getElementById('hq-tab-' + area);
var hasLoaded = tab.getAttribute('loaded');
if (!hasLoaded) {
tab.innerHTML = '<div style="margin:122px auto 0;width:250px;" ><img src="http://www1.pconline.com.cn/2011/product/img/loading5.gif" width="20" height="20" style="vertical-align:middle" > ' + areaName + '行情信息加载中，请稍候...</div>';
var head = document.getElementsByTagName("head")[0] || document.documentElement,
script = document.createElement("script");
script.src = 'http://www.pconline.com.cn/index_area2012/' + area + '.js';
head.appendChild(script);
}
}
});
/*全国行情块*/
var hqMoreOpenCities = document.getElementById('hqMoreOpenCities'),
hqMoreCitiesList = document.getElementById('hqMoreCitiesList');
hqMoreOpenCities.onmouseover = function(){
var self = hqMoreCitiesList;
var cName = 'pcPopHover'
if(self.className.indexOf(cName) != -1) return true;
clearTimeout(self.timer);
self.timer = setTimeout(function(){
self.className += ' ' + cName;
},150);
};
hqMoreOpenCities.onmouseout = function(){
clearTimeout(hqMoreCitiesList.timer);
hqMoreCitiesList.className='pcPop';
}
bindMouseLeave(hqMoreCitiesList, function(){
this.className='pcPop'
});
})();
</script>
