function bindMouseEnter(obj, fn) {
if (obj.attachEvent) {
obj.attachEvent('onmouseenter', function(){
fn.call(obj, window.event);
});
} else {
obj.addEventListener('mouseover', function(e) {
var rt = e.relatedTarget;
//alert(!isContain(obj, rt))
if (rt !== obj && !isContain(obj, rt)) {
fn.call(obj, e);
}
}, false);
}
}
function pcHover(items,targetItems,cName,lazy){
if(!items) return;
cName = cName ? cName : 'hover';
lazy = lazy ? lazy : 150;
for(var i=0,l = items.length; i<l;i++){
(function(n){
bindMouseEnter(items[n],function(e){
var self = this;
if(self.className.indexOf(cName) != -1) return true;
clearTimeout(self.timer);
self.timer = setTimeout(function(){
self.className += ' ' + cName;
},lazy); 
})
bindMouseLeave(items[n],function(e){
var self = this;
if(!self) return true;
clearTimeout(self.timer);
//clearTimeout(self.timer);
self.className = self.className.replace(' ' + cName , '');
})
})(i);
}
}
(function(){
/*切换行情块*/
var cityList = document.getElementById('hqCitiesList');
bindMouseLeave(cityList, function(){
this.className='pcPop';
});
var items = document.getElementById('JBBS_ul').getElementsByTagName('li');
pcHover(items);
items = pc.getElems('#JPDWrap li');
var ob = 20;
pc.each(items,function(item,i){
pc.addEvent(item,'mouseenter',function(e){
if(item.className.indexOf('hover') != -1) return true;
var ot = item.offsetTop,ich = item.clientHeight,cont = pc.getElems('.moreInfo',item)[0];
var sh = document.documentElement.clientHeight,st = document.body.scrollTop ? document.body.scrollTop : document.documentElement.scrollTop;
var JCountHeight = pc.getElem('#JCountHeight');
JCountHeight.innerHTML = '<div class="moreInfo">'+cont.innerHTML+'</div>';
pc.show(JCountHeight);
ch = JCountHeight.clientHeight;
pc.hide(JCountHeight);
var maxTop = (ch - ich) < 0 ? 0 : (ch - ich);
var result = sh - (ot - st) - ch
if(result > ob ){
pc.setStyle(cont,{top: 0 + 'px'});
}else if(result < ob && result - ob > - maxTop){
pc.setStyle(cont,{top: (result - ob) + 'px'});
}else if(result - ob < - maxTop){
pc.setStyle(cont,{top: - maxTop + 'px'});
}
clearTimeout(item.timer);
item.timer = setTimeout(function(){
pc.addClass(item,'hover');
},150);
});
pc.addEvent(item,'mouseleave',function(e){
clearTimeout(item.timer);
pc.removeClass(item,'hover');
})
});
//用于内容向上滚动,有停顿
/**startmarquee(一次滚动高度,速度,停留时间,元素ID);**/
function startmarquee(lh,speed,delay,obj){
var t;
var p=false;
var o=document.getElementById(obj);
if (!o) return true;
o.innerHTML+=o.innerHTML;
o.onmouseover=function(){p=true}
o.onmouseout=function(){p=false}
o.scrollTop = 0;
function start(){
t=setInterval(scrolling,speed);
if(!p) o.scrollTop += 1;
}
function scrolling(){
if(o.scrollTop%lh!=0){
o.scrollTop += 1;
if(o.scrollTop>=o.scrollHeight/2) o.scrollTop = 0;
}else{
clearInterval(t);
setTimeout(start,delay);
}
}
setTimeout(start,delay);
}
startmarquee(26,100,2000,"divScroll");//滚动
//startmarquee(24,60,2000,"scrollTxt");
startmarquee(24,60,2000,"scrollContent");
