(function(c,d){var i=40000,n=100,h=1;var s=c.document,o=/CSS1Compat/.test(s.compatMode),g=/webkit/ig.test(navigator.userAgent),e=/firefox/ig.test(navigator.userAgent),f=o&&!g?s.documentElement:s.body,b=s.documentElement,q=b.clientHeight,l=f.scrollTop,m=function(k){var t=(k&&k.getBoundingClientRect&&k.getBoundingClientRect().top)||0;return t==0?null:Math.floor((t+f.scrollTop)/100)},a=function(k){while(k){var t=m(k);if(t!=null){return t}k=k.parentNode}return null};var r={add:function(u,t,k){if(u.attachEvent){u["e"+t+k]=k;u[t+k]=function(){u["e"+t+k](c.event)};u.attachEvent("on"+t,u[t+k])}else{u.addEventListener(t,k,false)}},remove:function(u,t,k){if(u.detachEvent){u.detachEvent("on"+t,u[t+k]);u[t+k]=null}else{u.removeEventListener(t,k,false)}}};var p={imgs:[],textareas:[],hideImgs:[],actions:[],analysisImg:function(x){var A=document.images,v=A.length;for(var t=0;t<v;t++){if(A[t]&&A[t].getAttribute("#src")){var w=A[t].XX,u=A[t].YY,z,k=this.imgs;if(u===null){continue}z=m(A[t]);if(z===null){A[t].YY=null;this.hideImgs.push(A[t]);continue}if(z===u&&!x){continue}if(u!==d){if(k[u]!==d&&k[u][w]!==d){k[u][A[t].XX]=d}}if(k[z]===d){k[z]=[]}A[t].XX=k[z].push(A[t])-1;A[t].YY=z}}this.preLoad()},loadImgs:function(w,t){var u=this,k=this.imgs,v=t-w;u.cross(u.parseImgs,[w,t],[w,0],[t,t+2*v]);p.imgFlag=true},parseImgs:function(u){if(p.imgs[u]!=d){var k=p.imgs[u].length;for(var t=0;t<k;t++){if(!p.setSrc(p.imgs[u][t])){continue}}p.imgs[u]=d}},setSrc:function(t){var k;if(!t){return false}k=t.getAttribute("#src");if(!k){return false}t.src=k;t.removeAttribute("#src");return true},parseHideImgs:function(){var u=p.hideImgs,k=u.length;for(var t=0;t<k;t++){if(!p.setSrc(u[t])){continue}}},preLoad:function(){var k=Math.round(b.clientHeight/100);p.cross(p.parseImgs,[0,Math.round(k*h)])},attach:function(){var k=this;r.add(c,"scroll",function(){k.fire()});r.add(c,"resize",function(){k.fire()});k.fire();return this},fire:function(){var k=this;if(this.scrollTimer!=d){c.clearTimeout(this.scrollTimer)}this.scrollTimer=c.setTimeout(function(){if(!p.winLoaded&&Math.abs(f.scrollTop-l)>q){p.analysisImg()}l=f.scrollTop;q=b.clientHeight;var u=Math.floor(l/100),t=Math.floor(q/100);k.loadImgs(u,u+t)},n)},cross:function(y){var k=arguments,t=k.length;if(t<=1){return}for(var u=1;u<t;u++){var x=k[u][0]>k[u][1]?-1:1;while(k[u][0]!=k[u][1]){var v=k[u][0],w=k[u][1];y(v);k[u][0]+=x}}}};window.setTimeout(function(){p.analysisImg();p.attach()},0);for(var j=1;j<=20;j+=j){c.setTimeout(function(){if(!p.winLoaded){p.analysisImg()}},j*500)}r.add(c,"load",function(){p.winLoaded=true;p.analysisImg(true);p.parseHideImgs()});r.add(f,"click",function(){});c.lazy=p})(window);
function needJS(fn,src,callback) {
callback=callback||function(){};
if(fn) return callback(false);//功能早已载入
var scripts=document.___needJS__||(document.___needJS__=[]);
var script=scripts[src]||(scripts[src]={loaded:false,callbacks:[]});
if(script.loaded) return callback(false); //取得全局加载队列中的目标项。__needJS__和callbacks变量名绝不能改！！！
var cbs=script.callbacks;
if(cbs.push(callback)==1) {
var js=document.createElement("script");
js.onload=js.onreadystatechange=function(){
var st=js.readyState;
if(st&&st!="loaded"&&st!="complete") return;
script.loaded = true;
for(var i=0; i<cbs.length; i++) cbs[i](true);
};
js.src=src;
document.getElementsByTagName("head")[0].appendChild(js);
}
} //needJS() 本函数定义的代码可直接复制到其它地方使用
</script>
