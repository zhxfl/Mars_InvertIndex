<script type="text/javascript">
deviceJump.init({
'wap_3g' : 'http://g.pconline.com.cn/#frompc',
'wap' : 'http://g.pconline.com.cn/w/'
},function(name,jsonUrl){
if(document.referrer.toLowerCase().match(/pconline/i) == "pconline"){
delete jsonUrl[name];
}
if (name == "ipad") {
collect();
ipad_ivy.style.display = "block";
}
});
//弹出收藏窗口
/**
+------------------------------------------------------------------------------
1. 用户每天打开网站弹出一次提示框
+------------------------------------------------------------------------------
* @tarymee
+------------------------------------------------------------------------------
*/
function collect() {
var collect_wrap = document.getElementById('collect_wrap'),
collect_c = document.getElementById('collect_c'),
collect_cA = document.getElementById('collect_cA'),
collect_btn = document.getElementById('collect_btn'),
ipad_ivy = document.getElementById('ipad_ivy');
var d= new Date();
var nowDate = d.getDate();
if (nowDate == localStorage["firstDate"]) {
collect_wrap.style.display = 'none';
}else {
try{
localStorage["firstDate"] = d.getDate();
collect_wrap.style.display = 'block';
}catch(err){}
}
collect_c.onclick = function () {
collect_wrap.style.display = 'none';
}
collect_cA.onclick = function () {
collect_wrap.style.display = 'none';
}
collect_btn.onclick = function () {
collect_wrap.style.display = 'none';
window.location.href = "http://ipad.pconline.com.cn/";
}
}
</script>
