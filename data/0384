function loadDealerInfo() {
	try{
		if( undefined == __ipLocationInfo ){
			var _pro_id = 1;
			var _city_id  = 475;
			console.log( 'load defualut beijing' );
		} else {
			var _pro_id = ( undefined == __ipLocationInfo.province_id ) ? 1 : __ipLocationInfo.province_id;
			var _city_id = ( undefined == __ipLocationInfo.city_id ) ? 475 : __ipLocationInfo.city_id;
		}
		var dealerapiurl = "http://www.xcar.com.cn/cms/dealer.php?city_id=" + _city_id + '&pserid=' + 162 + '&type=' + 2;
		document.getElementById( 'area3' ) .href = 'http://dealer.xcar.com.cn/info/' + _pro_id +'_'+ _city_id +'_0_0_0_0/';
		loadScript( dealerapiurl , 
			function(){
			   var dnewstr = '';
				if( typeof dealerNewsInfo == 'object' ) {
					for( var i =0,j = dealerNewsInfo.length ; i < j ; i++ ) {
						dnewstr += '<li><a href="' + dealerNewsInfo[i].url + '" target="_blank" title="' + dealerNewsInfo[i].title_ln + '">' + dealerNewsInfo[i].title + '</a></li>';
					}
					document.getElementById("dealerBuyCarNews").innerHTML = dnewstr ;
				}
		    }
				
		);
	}catch(e){}				
}
