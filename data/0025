<script type="text/html" id="guessModeListHTML">
	<ul class="videos">
	<%for (var i = 0, len = data.length ,oExp = exp , r = rpid; i < len && i < 12; i++) { var item = data[i],nIndex = i+1,word = escape(item.title);
		var totalMin = item.duration;
		var mins = Math.floor(totalMin/60);
		var sends = totalMin%60;
		if(mins>=60&&mins>0){
			var hour = Math.floor(mins/60);
			mins = mins%60;
			if(hour!=''&& hour<=10){
				hour = '0'+hour+':';
			}
			else{
				 hour = hour+':';
			}
		}else{
			hour='';	
		}	
		if( mins<10){
			mins = '0'+mins;
		}
		if(sends<10){
			sends = '0'+sends;
		}
		var time = hour + mins +':' + sends;
	
	 %>

		<li class="<% if ( nIndex % 6 === 0 ) { %> row-last<% } %>">
			<a static="stp=hb&exp=<%=oExp%>&rpid=<%=r%>&id=li-<%=i+1%>&row=<% if ( nIndex <=6 ) { %>1<% } %><% if ( nIndex >6 ){ %>2<% } %>&col=<% if ( nIndex <=6 ) { %><%=nIndex%><% } %><% if ( nIndex >6 ){ %><%=nIndex-6%><% } %>" href="http://v.baidu.com/kan/ugcrp/?topn=12&iid=<%=item.iid%>&title=<%=item.tgbk%>&url=<%=escape(item.url)%>&frp=guess" target="_blank" class="poster" title="<%=item.title%>">

				<img src="http://t1.baidu.com/it/u=<%=item.img_sign1%>,<%=item.img_sign2%>&amp;fm=20" alt="<%=item.title%>" />
				<span class="v-play-icon"></span>				
			</a>
			<div class="v-desc">
				<dl>
					  <dt class="v-title">
						 <a static="stp=title&exp=<%=oExp%>&rpid=<%=r%>&id=li-<%=i+1%>&row=<% if ( nIndex <=6 ) { %>1<% } %><% if ( nIndex >6 ){ %>2<% } %>&col=<% if ( nIndex <=6 ) { %><%=nIndex%><% } %><% if ( nIndex >6 ){ %><%=nIndex-6%><% } %>" title="<%=item.title%>" target="_blank" href="http://v.baidu.com/kan/ugcrp/?topn=12&iid=<%=item.iid%>&title=<%=item.tgbk%>&url=<%=escape(item.url)%>&frp=guess"><%=item.title%></a>
