  },function(){
    var index = $(this).index();    
    var timeId = setTimeout(function(){$info.eq(index).removeClass("current");},30);

    $info.hover(function(){
      clearTimeout(timeId);
    },function(){
      $(this).removeClass("current");
    });
  });

