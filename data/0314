	var Xiangce = (function(){
									var maxLen = 9;
							var buildPiclist = function(){
			var url = "http://xiangce.baidu.com/picture/data/recentlist?portrait=75606261696b656775616e66616e67bc00";	
			qing.sio.callByServer(url,function(data){
				var elXiangce = qing.dom.query('.mod-xiangce')[0];

				if(data.status.code == 0 && data.data.length) {
					var piclist = data.data;
					var arr = [];
					for(var i=0,len=piclist.length;i<len && i < maxLen;i++){
						arr.push('<li title="' + T.encodeHTML(piclist[i].pic_desc)  + '">');
						arr.push('<a href="' + T.encodeHTML(piclist[i].pic_detail) + '" target="_blank">');
						arr.push('<img src="' + piclist[i].pic_src + '" />');
						arr.push('</a></li>');
					}
					// 这种情况下表示用户已登录，并且是自己看自己的页面
