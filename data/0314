						arr.push('</a></li>');
					}
					qing.g('ulPicList').innerHTML = arr.join('');
					qing.dom.removeClass(elXiangce,'hide');
				}	
			});
		};

		var init = function(){
			qing.dom.ready(function(){
				buildPiclist();	
			});
		};

		return {
			init : init
		};
	})();
	
