            <div class="image-list">
                <div class="image-list-wrapper" id="media_list">

                </div>
            </div>
        </div>
</div>
<script type='text/javascript'>
    F.use('/static/common/lib/tangram/base/base.js' ,function(T){
        var baidu = T;
        baidu.dom.ready(function(){
        var s=0,
            data,
            tpl = '<div class="image-list-item"><a href="http://news.baidu.com/n?cmd=1&class=media##{id}" title="#{name}" target="_blank" mon="name=chuanmei"><img src="#{thumb}"></a></div>';
        baidu.ajax.get('/n?m=rddata&v=media_list',function(r,d){

            data = baidu.json.parse(d);

            var len = data.data.length;

            s = 5 * Math.floor( Math.random() * Math.floor( len / 5 ) );
            baidu.dom.g('chuanmei_link').href += '#' + Math.ceil( Math.random() * len);

            baidu.on('media_refresh','click', function(){
                if(s>=data.data.length) s=0;
                buildHTML();
            });
            buildHTML();
        });


        function buildHTML(){
        var temp = data.data.slice(s,s+5),h='';
            baidu.each(temp,function(item){
                h+=baidu.string.format(tpl,item);
            });
            baidu.dom.empty('media_list');
            baidu.dom.g('media_list').innerHTML = h;
            s+=5;
        }
    });
});
</script>


