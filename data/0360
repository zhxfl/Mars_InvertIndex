var scroll_timer;
function AutoScroll(){
$('#s1').find("ul:first").animate({
marginTop:"-25px"
},500,function(){
$(this).css({marginTop:"0px"}).find("li:first").appendTo(this);
});
}
$(document).ready(function(){
scroll_timer = setInterval(AutoScroll,3000);
});

$("#s1 a").mouseover(function(){
    clearInterval(scroll_timer)
}).mouseout(function(){
    scroll_timer = setInterval(AutoScroll,3000)
 });
