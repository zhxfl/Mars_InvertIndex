<script>
var readonly = 0; //107表示只读，0表示可发贴
var titleMax = 62; //最长吧名
var mypop;//弹出层
var postTitle = "";
CheckData.titlePass = false;
PageData["need_captcha"] = 0;
var int_h = parseInt(location.hash.slice(1)); //tab项初始化
if(!isNaN(int_h) && int_h>=1 && int_h<=5) SelectTab.curIndex = int_h;
SelectTab.init(SelectTab.curIndex);
/*全吧只读设置（应急时使用）*/
if(readonly == 107)
{
	mypop = new Popup({width:370,height:170,isReloadOnClose:false,isShowShadow:true});
	ctHtml = '<div class=mypop><center>抱歉！您目前不能发起投诉！</center><p>由于系统维护，暂时不能发起任何投诉。由此给您带来的不便我们深表歉意。请您耐心等待系统恢复正常。谢谢！</p></div><div style="text-align:center;"><input type="button" onclick="closePopup2();" value="确定"/></div>';
	mypop.html(ctHtml,"我要投诉");
	TsPost.setDisable();
}
/*没有登陆，则不能投诉*/
TsPost.setDisable();//firefox刷新页面时，缓存在输入框的数据影响了输入框是否为空的验证，所以先清空input
var forms = Fe.G("forms").getElementsByTagName("form");
for(i=0; i<forms.length; i++){ forms[i].reset();}
var params = $.tb.getSearch();
var notThread = !params.content && !params.pid && !params.id;
if (notThread) {
	$("#tabs li:eq(3)").click(function () {
		$("#form8-corner").hide();
		$("#redirect-form8").show();
	});
}
if (SelectTab.curIndex=='4') {
	if (!notThread) {
		$("#form8_link1").val("http://tieba.baidu.com/p/"+params.id+"?pid="+params.pid + "#"+params.pid).attr("disabled","disabled");
		$("#form8_uname").val(PageData.user.name).attr("disabled","disabled");
		var tousu_date = new Date(params.time*1000);
		$("#form8_postdate").val(tousu_date.getFullYear()+"-"+(tousu_date.getMonth()-0+1)+"-"+tousu_date.getDate()+" "+tousu_date.getHours()+":"+tousu_date.getMinutes()+":"+tousu_date.getSeconds());
		$("#form8_link2,#form8_link3,#form8_detail,.j_post_num,#form8_uname,#form8_postdate").attr("disabled","disabled").attr("style","background-color:#F5F5F5;border:1px solid darkGray");
	} else {
		$("#tabs li:eq(3)").click();
	}
}
