<script  type="text/javascript">
var threadOut = -1;
function tt(){
var ss;
$(".ul_1 li").mouseover(function(e){
//le= document.getElementById("ul_1").getElementsByTagName("li").length;
li_id=$(this).attr("id");
ss= setTimeout('st(li_id)',1000);
})
$(".ul_1 li").mouseout(function(){
clearTimeout(ss);
/*
le= document.getElementById("ul_1").getElementsByTagName("li").length;
for(i=1;i<=le;i++){
$("#tc_"+i).css("display","none");   
}
*/
});
$(".tc").mouseover(function(e){
    //e.cancelBubble = true;
    //event.cancelBubble = true;
    if (threadOut != -1) {
    clearTimeout(threadOut);
    threadOut = -1;
    }
    $(this).css('display','block'); 
    var channel = $(this).attr('channel');
    if(channel && !$(this).attr('got')){
      $(this).attr('got',1);
      $.getJSON('/index.php?action=index-livedatabychannel',{channel:channel},function(json){
          if(json.live.short_title){
              $('span[livechannel='+channel+']').text(json.live.sch_title);
              $('a[livechannel='+channel+']').attr('title',json.live.sch_title);
          }
      });
      var tc = $(this);
      setTimeout(function(){
            tc.removeAttr('got');
      },90000);
    } 
})
$(".tc").mouseout(function (e) {
//e.cancelBubble = true;
//event.cancelBubble = true;
threadOut = setTimeout(function(e){
threadOut = -1;
  le= document.getElementById("ul_1").getElementsByTagName("li").length;
for(i=1;i<=le;i++){
$("#tc_"+i).css("display","none");   
}
}, 20);
})
}
var n=0;
function st(){
	li_th_id=li_id.replace(/li/,"");
	le= document.getElementById("ul_1").getElementsByTagName("li").length;
	for(i=1;i<=le;i++){
		if(i!=li_th_id){
			$("#tc_"+i).css("display","none");   
		}else{
			var img = $("#tc_"+li_th_id).css("display","block").find('img');
			var pic = img.attr('data');
			if(pic != img.attr('src')) {
				img.attr('src', pic);
			}
		}
	}
}
 
 
 
tt();
 


</script>
<script type="text/javascript">
/*显示输入框内容*/
function showContent(p){
  var s_box = document.getElementById("cont_box");
  if(s_box.style.display==""){s_box.style.display="none";}
  s_box.style.display = s_box.style.display=="none"?"block":"none";
}
//鼠标点击隐藏下拉窗口
function hideSelect(e){
  var evn = kEvent1.getTarget(e);
  if(evn.parentNode){
    if(evn.parentNode.parentNode){
          if(evn.parentNode.parentNode!=document){
            if(
               evn.parentNode.getAttribute("id")=="cont_box" 
               ) {
              return;
            }
          }
    }
  }

document.getElementById('cont_box').style.display = 'none';
}

kEvent1.addEvent("bid","mousedown",hideSelect);



(new module('sample_dsj')).initTabsHover();
(new module('sample_dy')).initTabsHover();
(new module('sample_dhp')).initTabsHover();
(new module('sample_pindaopaihang')).initTabsHover();

</script>
<script>
function lb_js(){
	
	
	size=$(".lunbo_ts_120907 .ul_1z li").size();
	
	
	$(".lunbo_ts_120907 .ul_1z li").each(function(i){
								  				
				if($(".lunbo_ts_120907 .ul_1z li").eq(i).attr("class")=="cur"){
					$(".lunbo_ts_120907 .ul_1z li").eq(i).attr("class","");
					

					i+=1;
					
					$(".lunbo_ts_120907 .image .qh").css("display","none");
					$(".lunbo_ts_120907 .image #qh_0_"+i).css("display","block");
					data=$(".lunbo_ts_120907 .image #qh_0_"+i+" img").attr("data");
				    $(".lunbo_ts_120907 .image #qh_0_"+i+" img").attr("src",data);

					
					$(".lunbo_ts_120907 .ul_1z li").eq(i).attr("class","cur");
					
					if(i==9){
					$(".lunbo_ts_120907 .ul_1z li").eq(0).attr("class","cur");
					 $(".lunbo_ts_120907 .ul_2z li").eq(0).attr("class","cur");
					 $(".lunbo_ts_120907 .ul_2z li").eq(1).attr("class","");
					 $(".lunbo_ts_120907 .ul_2z li").eq(2).attr("class","");
					  $(".lunbo_ts_120907 #ul_0z").css("display","block");
					  $(".lunbo_ts_120907 #ul_1z").css("display","none");
					 $(".lunbo_ts_120907 #ul_2z").css("display","none");
					 
					 $(".lunbo_ts_120907 .image .qh").css("display","none");
					$(".lunbo_ts_120907 .image #qh_0_"+0).css("display","block");
					data=$(".lunbo_ts_120907 .image #qh_0_"+0+" img").attr("data");
				    $(".lunbo_ts_120907 .image #qh_0_"+0+" img").attr("src",data);
					 
					 
			
						}
						
						
						
				//切换列表下面的分页
				 if(i==3){
					 $(".lunbo_ts_120907 .ul_2z li").eq(0).attr("class","");
					 $(".lunbo_ts_120907 .ul_2z li").eq(1).attr("class","cur");
					 $(".lunbo_ts_120907 .ul_2z li").eq(2).attr("class","");
					  $(".lunbo_ts_120907 #ul_0z").css("display","none");
					   $(".lunbo_ts_120907 #ul_1z").css("display","block");
					 $(".lunbo_ts_120907 #ul_2z").css("display","none");
					 }
					 
					 
					 if(i==6){
					 $(".lunbo_ts_120907 .ul_2z li").eq(0).attr("class","");
					 $(".lunbo_ts_120907 .ul_2z li").eq(1).attr("class","");
					 $(".lunbo_ts_120907 .ul_2z li").eq(2).attr("class","cur");
					  $(".lunbo_ts_120907 #ul_0z").css("display","none");
					  $(".lunbo_ts_120907 #ul_1z").css("display","none");
					 $(".lunbo_ts_120907 #ul_2z").css("display","block");
					 }
					 
				
					return false;
					
					}else{
					
					
						}
				

				
												  });
	
	
	
	

	}








function ttz(){
	var $=jQuery;
	var time=3000;
	//自动
	
	var lb_js_c=setInterval("lb_js();",time);
	
	
	
	$(".lunbo_ts_120907").mouseenter(function(){
											  
				clearInterval(lb_js_c);					  
											  
											  
											  });
	
	$(".lunbo_ts_120907").mouseleave(function(){
											  
				lb_js_c=setInterval("lb_js();",time);					  
											  
											  
											  });
	
	
	
	
	

	
	

	$(".lunbo_ts_120907 .ul_2z li").click(function(){
	//		$(".lunbo_ts_120907 .lb_lb .ul_1z li").attr("class","");	
			$(".lunbo_ts_120907 .ul_2z li").attr("class","");
			$(this).attr("class","cur");
			
			$(".lunbo_ts_120907 .ul_2z li").each(function(i){
												  
						if($(this).attr("class")=="cur"){
							
					
					
							$(".lunbo_ts_120907 #ul_"+i+"z").css("display","block");
					//		$(".lunbo_ts #ul_"+i+" li").eq(0).attr("class","cur");
					
					
					
					    if(i==0){
								$(".lunbo_ts_120907 .ul_1z li").attr("class","");
								$(".lunbo_ts_120907 .ul_1z li").eq(0).attr("class","cur");
								 $(".lunbo_ts_120907 .image .qh").css("display","none");
							
								$(".lunbo_ts_120907 .image #qh_0_"+0).css("display","block");
					            data=$(".lunbo_ts_120907 .image #qh_0_"+0+" img").attr("data");
				                $(".lunbo_ts_120907 .image #qh_0_"+0+" img").attr("src",data);
					 
								
								
								
		
								}
					
					
					
					        if(i==1){
								$(".lunbo_ts_120907 .ul_1z li").attr("class","");
								$(".lunbo_ts_120907 .ul_1z li").eq(3).attr("class","cur");
								 $(".lunbo_ts_120907 .image .qh").css("display","none");
								
								$(".lunbo_ts_120907 .image #qh_0_"+3).css("display","block");
					            data=$(".lunbo_ts_120907 .image #qh_0_"+3+" img").attr("data");
				                $(".lunbo_ts_120907 .image #qh_0_"+3+" img").attr("src",data);
								
		
								}
								
								if(i==2){
								$(".lunbo_ts_120907 .ul_1z li").attr("class","");
								$(".lunbo_ts_120907 .ul_1z li").eq(6).attr("class","cur");
								 $(".lunbo_ts_120907 .image .qh").css("display","none");
								
							    $(".lunbo_ts_120907 .image #qh_0_"+6).css("display","block");
					            data=$(".lunbo_ts_120907 .image #qh_0_"+6+" img").attr("data");
				                $(".lunbo_ts_120907 .image #qh_0_"+6+" img").attr("src",data);
								
		
								}
								
								
					
					
					
							
							}else{

								$(".lunbo_ts_120907 #ul_"+i+"z").css("display","none");
								}						  
								
								
												  
												  });
			
										
										
					return false;					
										   
										   
										   
										   
									   });
	$(".lunbo_ts_120907 .lb_lb .ul_1z li").click(function(){
			
			$(".lunbo_ts_120907 .lb_lb .ul_1z li").attr("class","");				  
			$(this).attr("class","cur");
			
			$(".lunbo_ts_120907 .lb_lb .ul_1z li").each(function(k){
														
									if($(this).attr("class")=="cur"){
										
	
										$(".lunbo_ts_120907 .image .qh").css("display","none");
										$(".lunbo_ts_120907 .image #qh_0_"+k).css("display","block");
										data=$(".lunbo_ts_120907 .image #qh_0_"+k+" img").attr("data");
										$(".lunbo_ts_120907 .image #qh_0_"+k+" img").attr("src",data);

	
								
										
									}
														 
														 
														 });
			

	
					return false;						   
											   
											   
											   });
	
	
	
	
	}
ttz();

</script>

<script>

   var $=jQuery;
   var cur=0;
   var ssii=$(".pic .img01").size();
   var Speed = 10; //速度(毫秒)
    var Space = 10; //每次移动(px)
    var PageWidth = 890; //翻页宽度
    var fill = 0; //整体移位
    var MoveLock = false;
    var MoveTimeObj;
    var Comp = 0;
    var AutoPlayObj = null;
	
	
	
    GetObj("List2").innerHTML = GetObj("List1").innerHTML;
    GetObj('ISL_Cont').scrollLeft = fill;
    GetObj("ISL_Cont").onmouseover = function(){clearInterval(AutoPlayObj);}
    
	
	
	
    function GetObj(objName){if(document.getElementById){return eval('document.getElementById("'+objName+'")')}else{return eval
    
    ('document.all.'+objName)}}
    
    function ISL_GoUp(){ //上翻开始
	dq=ssii-=1;
	
	//ss=$(".pic .img01").size();
	
	if(dq!=0){	

	d=$(".pic .img01 img").eq(dq).attr("data");
	$(".pic .img01 img").eq(dq).attr("src",d);
	
	d2=$(".pic .img01 img").eq(dq+1).attr("data");
	$(".pic .img01 img").eq(dq+1).attr("src",d2);
    

	

}else{
	}
	
	
    if(MoveLock) return;
    clearInterval(AutoPlayObj);
    MoveLock = true;
    MoveTimeObj = setInterval('ISL_ScrUp();',Speed);
	
    }
	
	
    function ISL_StopUp(){ //上翻停止

    clearInterval(MoveTimeObj);
    if(GetObj('ISL_Cont').scrollLeft % PageWidth - fill != 0){
    Comp = fill - (GetObj('ISL_Cont').scrollLeft % PageWidth);
    CompScr();
    }else{
    MoveLock = false;
    }
    
    }
    function ISL_ScrUp(){ //上翻动作
    if(GetObj('ISL_Cont').scrollLeft <= 0){GetObj('ISL_Cont').scrollLeft = GetObj
    
    ('ISL_Cont').scrollLeft + GetObj('List1').offsetWidth}
    GetObj('ISL_Cont').scrollLeft -= Space ;
	ISL_StopUp();
    }
	
	
	
    function ISL_GoDown(){ //下翻
	
	dq=cur+=1;
	ss=$(".pic .img01").size();
	//ss=ss/2-1;
	if(dq<=ss-1){	
	d=$(".pic .img01 img").eq(dq).attr("data");
	$(".pic .img01 img").eq(dq).attr("src",d);

}else{
	}

    clearInterval(MoveTimeObj);
    if(MoveLock) return;
    clearInterval(AutoPlayObj);
    MoveLock = true;
    ISL_ScrDown();
	
	
	
	
    MoveTimeObj = setInterval('ISL_ScrDown()',Speed);
	
    }
	
	
	
    function ISL_StopDown(){ //下翻停止
    clearInterval(MoveTimeObj);
    if(GetObj('ISL_Cont').scrollLeft % PageWidth - fill != 0 ){
    Comp = PageWidth - GetObj('ISL_Cont').scrollLeft % PageWidth + fill;
    CompScr();
    }else{
    MoveLock = false;
    }
    
    }
    function ISL_ScrDown(){ //下翻动作
    if(GetObj('ISL_Cont').scrollLeft >= GetObj('List1').scrollWidth){GetObj('ISL_Cont').scrollLeft =
    
    GetObj('ISL_Cont').scrollLeft - GetObj('List1').scrollWidth;}
    GetObj('ISL_Cont').scrollLeft += Space ;
	ISL_StopDown();
    }
	
	
	
    function CompScr(){
    var num;
    if(Comp == 0){MoveLock = false;return;}
    if(Comp < 0){ //上翻
    if(Comp < -Space){
       Comp += Space;
       num = Space;
    }else{
       num = -Comp;
       Comp = 0;
    }
    GetObj('ISL_Cont').scrollLeft -= num;
    setTimeout('CompScr()',Speed);
    }else{ //下翻
    if(Comp > Space){
       Comp -= Space;
       num = Space;
    }else{
       num = Comp;
       Comp = 0;
    }
    GetObj('ISL_Cont').scrollLeft += num;
    setTimeout('CompScr()',Speed);
    }
    }



var speed2=5000000000;
var man_si=setInterval('ISL_GoDown();',speed2);


function tt1(){
	var $=jQuery;
	
    d=$(".pic .img01 img").eq(0).attr("data");
	$(".pic .img01 img").eq(0).attr("src",d);

    

	$(".rollBox").mouseenter(function(){
				  clearInterval(man_si);				   
								   });


	$(".rollBox").mouseleave(function(){
							
		man_si=setInterval('ISL_GoDown();',speed2);  		   
								   });
	
	
	
	
	
	$(".right_pic").mouseenter(function(){
									
				  clearInterval(man_si);				   
								   });
	$(".right_pic").mouseleave(function(){
				man_si=setInterval('ISL_GoDown();',speed2);   
								   });
	
	
	
	
	
		$(".left_pic").mouseenter(function(){
									
				  clearInterval(man_si);				   
								   });
	$(".left_pic").mouseleave(function(){
				man_si=setInterval('ISL_GoDown();',speed2);   
								   });
	
	
	

	
	
	}
tt1();

</script>
