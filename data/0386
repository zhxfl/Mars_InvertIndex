var search_href = window.location.href;
var is_shouye = search_href == 'http://tv.cntv.cn/index.php' || search_href == 'http://tv.cntv.cn/' || search_href == 'http://tv.cntv.cn' ? 1 : 0;
var sp_id = is_shouye ? "&sid=1021&pid=0000":"&sid=0021&pid=0000";
var hotword = is_shouye ? "&sid=1021&pid=0002":"&sid=0021&pid=0002";
var search_a = document.getElementById('search_hot_id').getElementsByTagName('a');
for(i=0;i<search_a.length;i++){
	search_a[i].href = search_a[i].href + hotword;
}
function submitSearch(){
  var loc = location.href;
  var searchUrl = '';
  var keywords = document.getElementById("cont_ibox").value;
  var integer = /^\+?[1-9][0-9]*$/;
  var key = keywords.replace(/[^\d]*/ig,'').length;
  if(keywords.indexOf('条') != -1 && (key+1)==keywords.length){
    alert('请输入关键词');
  return false;
  }
  if(keywords.replace(' ','') == ''){
    alert('请输入关键词');
    return false;
  }
  var type = "-1";
  //searchUrl = searchUrl + encodeURIComponent(keywords) + sp_id;
  searchUrl = 'http://search.cctv.com/search.php?qtext=' + encodeURIComponent(keywords) + sp_id;
  window.open(searchUrl);
}
function getOnkeyDown(e){
var ev = e;
ev = ev || event;
if(ev.keyCode == 13){
submitSearch();
}
} 
