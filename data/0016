<script type="text/javascript" charset="utf-8">

var logMgr = (function () {
    var logName, oneKeyCallTimer = null,
		logUrl = 'http://wangdan.baidu.com/netorderimport/failsubmit.action';

    function getLogUrl(params) {
		var url = [],
			req = (new Date()).getTime();
		url.push(logUrl + '?req=' + req);
		(logName != null) && (url.push('module=' + logName));
		url.push(params);
		return url.join('&');
    }

    return {
		init : function (params) {
			logName = params.name;
			(params.url != null) && (logUrl = params.url);
		},
		
		send : function (params) {
			var reqObj = null,
			req = (new Date()).getTime(),
			image = window['_Img' + req] = new Image();
			image.onload = image.onerror = function () {
				window['_Img' + req] = null;
				image = null;
			};
			image.src = getLogUrl(params);
		},
		
		loopCheckOnekeyCall : function(params) {
			if (oneKeyCallTimer != null) {
				return;
			}
			oneKeyCallTimer = setInterval(function (){
				if(window.successTime) {
					logMgr.send(params + '&phone=' + window.submitPhone);
					window.successTime = null;
					window.submitPhone = null;
					clearInterval(oneKeyCallTimer);
					oneKeyCallTimer = null;
				}
			}, 500);
		}
    };
})();

(function () {
    var applyFrame = baidu.g('applyFrame'),
	applyUrl = {
	    'nar' : 'http://wangdan.baidu.com/netorderimport/onlineApplicationNarrow.action',
	    'nor' : 'http://wangdan.baidu.com/netorderimport/onlineApplication.action'
	}, refer = baidu.cookie.get('REFER_WD') ? baidu.cookie.get('REFER_WD') : '1';

    logMgr.init({name:'ebaidu'});

    if (baidu.g('oneKeyCallBtn')) {
        baidu.on(baidu.g('oneKeyCallBtn'), 'click', function(event) {
            showIframeFloater("oneKeyCallFloater", 
                "http://wangdan.baidu.com/onekeycall/showCallPageNew.action?r" + new Date().getTime() + "=1");      
            logMgr.loopCheckOnekeyCall('name=onekeycall&refer=' + refer);
            baidu.event.preventDefault(event);
        }); 
    }

    if (applyFrame) {
	applyFrame.src = applyUrl[applyFrame.getAttribute('ref')] + '?req=' + new Date().getTime() + '&refer=' + refer;
    }
})();
</script>
