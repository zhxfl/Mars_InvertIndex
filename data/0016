//成功案例切换
(function () {
    function changeCate() {
	var ele = this,
	    areas = ['trade', 'area'];
	for (var i = 0, id; id = areas[i]; i++) {
	    baidu.hide(id + 'Area');
	    baidu.g(id + 'Btn').className = baidu.g(id + 'Btn').className.replace(/\s+btn-st-cur/, '');
	}
	baidu.show(ele.href.split('#')[1] + 'Area');
	ele.className += ' btn-st-cur';
	return false;
    }
    var btns = baidu.g('cateBtnArea').getElementsByTagName('a');
    for (var i = 0, item; item = btns[i]; i++) {
	item.onclick = changeCate;
    }
})();

//搜索信息轮转
(function (){
    var target = baidu.g('searchInfo'),
	pageSize = 1,
	stepSize = 25 * pageSize,
	timer, dt = 6000;
    
    function go(c) {
		var dx = c > 0 ? -2 * stepSize : 0;
		if (timer != null) {
			clearTimeout(timer);
			timer = null;
	}

	Tween.run(target, 
	    {marginTop : dx }, 
	    300, 
	    'quadOut', 
	    function () {
		var ele, opt, tar, d;
		if (c > 0) {
		    ele = baidu.dom.first;
		    opt = baidu.dom.insertAfter;
		    tar = baidu.dom.last
		}
		else {
		    ele = baidu.dom.last;
		    opt = baidu.dom.insertBefore;
		    tar = baidu.dom.first;
		}
		for (var i = 0; i < pageSize; i++) {
		    opt(ele(target), tar(target));
		}
		//target.style.marginLeft = '-' + stepSize + 'px';
		target.style.marginTop = 0;
		timer = setTimeout(function () {go(c);}, dt);
	    }
	);
    }

    function loop() {
		timer = setTimeout(function() {
			go(1);
		}, dt);
    }

    baidu.dom.ready(function () {
	var lis = target.getElementsByTagName('li'),
	    len, i, a, html = [];

	loop();

    });
})();
</script>

